 <def-macro n="f_is_net" npar="1"> <!-- par1 = SV -->
   <choose>
     <when>
       <test>
         <or>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="conclure"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="dire"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="écrire"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="faire"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="lire"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="répondre"/></equal>
           <equal><clip pos="1" part="nbr"/><lit-tag v="pl"/></equal>
           <equal><clip pos="1" part="temps"/><lit-tag v="pii"/></equal>
         </or>
       </test>
       <let><var n="is_net"/><lit-tag v="yes"/></let>
     </when>
     <otherwise>
       <let><var n="is_net"/><lit-tag v="no"/></let>
     </otherwise>
   </choose>
 </def-macro>

 <def-macro n="generate_il" npar="1"> <!-- par1 = SV -->
   <choose>
     <when>
       <test>
        <equal><var n="is_net"/><lit-tag v="yes"/></equal>
       </test>
       <out>
         <chunk>
           <lit v="subj"/>
           <lit-tag v="prn.subj"/>
           <clip pos="1" part="pers"/>
           <lit-tag v="GD"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="-prpers_net"/>
           <lit-tag v="prn.tn.3.4.5"/>
           <lit v="$}"/>
         </chunk>
       </out>
     </when>
     <otherwise>
       <out>
         <chunk>
           <lit v="subj"/>
           <lit-tag v="prn.subj"/>
           <clip pos="1" part="pers"/>
           <lit-tag v="GD"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="-prpers"/>
           <lit-tag v="prn.tn.3.4.5"/>
           <lit v="$}"/>
         </chunk>
