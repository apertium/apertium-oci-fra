     </when>
     <otherwise>
       <out>
         <chunk>
           <lit v="det"/>
           <lit-tag v="Det"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="le"/>
