  <section-def-macros>

 <def-macro n="generate_de" npar="0" v="oci@gascon"> 
    <!-- ^de<PREP>{^de<pr>$}$ -->
   <out>
     <chunk>
       <lit v="de"/>
       <lit-tag v="PREP"/>
       <lit v="{^"/>
       <lit v="de"/>
       <lit-tag v="pr"/>
       <lit v="$}"/>
     </chunk>
     <b/>
   </out>
 </def-macro>

 <def-macro n="generate_detdef1" npar="1"> <!-- par1 = SN -->
   <choose>
     <when>
       <test>
         <equal><clip pos="1" part="a_np"/><lit-tag v="np"/></equal>
       </test>
       <!-- pas de generacion -->
     </when>
     <when>
       <test>
         <or>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="d_n"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="d_a_n"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="d_n_a"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="num_n"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="num_a_n"/></equal>
           <equal caseless="yes"><clip pos="1" part="lem"/><lit v="num_n_a"/></equal>
         </or>
       </test>
       <!-- pas de generacion -->
     </when>
     <when>
       <test>
         <equal><clip pos="1" part="nbr"/><lit-tag v="sg"/></equal>
       </test>
       <out>
         <chunk>
           <lit v="det"/>
           <lit-tag v="Det"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="le"/>
           <lit-tag v="det.def"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="$}"/>
         </chunk>
         <b/>
       </out>
     </when>
     <otherwise>
       <out>
         <chunk>
           <lit v="det"/>
           <lit-tag v="Det"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="le"/>
           <lit-tag v="det.def.mf.pl"/>
           <lit v="$}"/>
         </chunk>
         <b/>
       </out>
     </otherwise>
   </choose>
 </def-macro>

 <def-macro n="generate_detdef2" npar="2"> <!-- par1 = SN, par2 = SAdv -->
   <choose>
     <when>
       <test>
         <or>
           <equal><clip pos="2" part="a_pron_pas_neg"/><lit-tag v="pas_neg"/></equal>
           <in caseless="yes"><clip pos="2" part="lem"/><list n="adv_sens_det"/></in>
         </or>
       </test>
       <!-- pas de generacion -->
     </when>
     <when>
       <test>
         <equal><clip pos="1" part="nbr"/><lit-tag v="sg"/></equal>
       </test>
       <out>
         <chunk>
           <lit v="det"/>
           <lit-tag v="Det"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="le"/>
           <lit-tag v="det.def"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="$}"/>
         </chunk>
         <b/>
       </out>
     </when>
     <otherwise>
       <out>
         <chunk>
           <lit v="det"/>
           <lit-tag v="Det"/>
           <clip pos="1" part="gen"/>
           <clip pos="1" part="nbr"/>
           <lit v="{^"/>
           <lit v="le"/>
           <lit-tag v="det.def.mf.pl"/>
           <lit v="$}"/>
         </chunk>
         <b/>
       </out>
     </otherwise>
   </choose>
 </def-macro>

 <def-macro n="generate_detdef3" npar="3"> <!-- par1 = SN, par2 = SAdv, par3 = SAdv -->
   <choose>
     <when>
       <test>
         <or>
           <equal><clip pos="2" part="a_pron_pas_neg"/><lit-tag v="pas_neg"/></equal>
           <in caseless="yes"><clip pos="2" part="lem"/><list n="adv_sens_det"/></in>

           <equal><clip pos="3" part="a_pron_pas_neg"/><lit-tag v="pas_neg"/></equal>
           <in caseless="yes"><clip pos="3" part="lem"/><list n="adv_sens_det"/></in>
