  <def-macro n="f_bcond" npar="2"><!--Per mirar si el blanc conté o no format. Cal posar aquesta funció en les regles en què desaparegui alguna paraula, per tal de decidir si el blanc de la paraula eliminada s'ha d'eliminar o conservar. Si conté format cal conservar-la, si no en conté cal eliminar-la perquè no apareguin dos espais seguits. -->
             <choose>
               <when>
                 <test>
                   <not>
                     <equal>
                       <b pos="1"/>
                       <lit v=" "/>
                     </equal>
              </not>
                 </test>
            <out>
              <b pos="1"/>
            </out>
          </when>
             </choose>
     </def-macro>

 <def-macro n="write_ce" npar="1"> 
        <choose>
          <when>
            <test>
              <and>
                <equal><clip pos="1" part="a_verb"/><lit-tag v="vbhaver"/></equal>
                <equal><clip pos="1" part="pers"/><lit-tag v="p3"/></equal>
                <equal><clip pos="1" part="nbr"/><lit-tag v="pl"/></equal>
              </and>
            </test>
            <out>
              <chunk>
                <get-case-from pos="1">
                  <lit v="subj"/>
                </get-case-from>
                <lit-tag v="prn.subj"/>
                <clip pos="1" part="pers"/>
                <clip pos="1" part="nbr"/>
                <lit v="{^"/>
                <get-case-from pos="1">
                  <lit v="cela"/>
                </get-case-from>
                <lit-tag v="prn.tn.nt"/>
                <lit v="$}"/>
              </chunk>
              <b/>
            </out>
          </when>
          <otherwise>
            <out>
              <chunk>
                <get-case-from pos="1">
                  <lit v="subj"/>
                </get-case-from>
                <lit-tag v="prn.subj"/>
                <clip pos="1" part="pers"/>
                <clip pos="1" part="nbr"/>
                <lit v="{^"/>
                <get-case-from pos="1">
                  <lit v="ce"/>
                </get-case-from>
                <lit-tag v="prn.tn.nt"/>
                <lit v="$}"/>
              </chunk>
              <b/>
            </out>
          </otherwise>
        </choose>
     </def-macro>

 <def-macro n="gen_ce" npar="1"> 
    <choose>
      <when>
        <test>
          <and>
            <equal><var n="finfrasa"/><lit v="yes"/></equal>
            <not><equal><clip pos="1" part="temps"/><lit-tag v="imp"/></equal></not>
          </and>
        </test>
        <call-macro n="write_ce"><with-param pos="1"/></call-macro>
        <choose>
          <when>
            <test>
              <equal><case-of pos="1" part="lem"/><lit v="Aa"/></equal>
            </test>
            <modify-case><clip pos="1" part="lem"/><lit v="aa"/></modify-case>
          </when>
        </choose>
      </when>
   </choose>
 </def-macro>

 <def-macro n="gen_ce_incond" npar="1"> 
