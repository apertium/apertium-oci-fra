    <match lemma="à moitié" tags="adv" v="oci@gascon"><select lemma="a miejas"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="à moitié" tags="adv" v="oci"><select lemma="mièg"/></match>
    <match lemma="à moitié" tags="adv" v="oci@gascon"><select lemma="mieitat"/></match>
    <or>
      <match tags="adj.*"/>
      <match tags="vblex.pp.*"/>
    </or>
  </rule>

  <!-- quelque >> environ, per tant -->
  <rule weight="0.8">
    <match lemma="quelque" tags="adv"><select lemma="environ"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="quelque" tags="adv"><select lemma="per tant"/></match>
    <or>
      <match tags="adj.*"/>
      <match tags="adv"/>
    </or>
  </rule>

  <!-- ne >> non, ne -->
  <rule weight="0.8">
    <match lemma="ne" tags="adv"><select lemma="non" tags="adv"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ne" tags="adv"><select lemma="ne" tags="adv.enon"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="ne" tags="adv"><select lemma="non" tags="adv"/></match>
    <match lemma="pas" tags="adv"/>
  </rule>

  <!-- pas >> pas, mai/shens -->
  <rule weight="0.8">
    <match lemma="pas" tags="adv"><select lemma="pas"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="pas" tags="adv"><select lemma="mai"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="*.pp.*"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.0">
    <match lemma="pas" tags="adv"><select lemma="shens"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="*.pp.*"/>
        <match tags="adv"/>
      </or>
    </repeat>
    <match lemma="que" tags="cnjsub"/>
  </rule>
  <rule weight="1.2">
    <match lemma="pas" tags="adv"><select lemma="pas"/></match>
    <match lemma="que" tags="cnjsub"/>
