      </or>
    </repeat>
    <match lemma="par" tags="pr"/>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <match lemma="père" tags="n.*"/>
      <match lemma="mère" tags="n.*"/>
      <match lemma="parent" tags="n.*"/>
      <match lemma="grand-père" tags="n.*"/>
      <match lemma="aïel" tags="n.*"/>
      <match lemma="arrière-grand-père" tags="n.*"/>
      <match lemma="oncle" tags="n.*"/>
      <match lemma="tante" tags="n.*"/>
    </or>
  </rule>

<!-- **************** ADV **************** -->

  <!-- traduire >> traduire/tradusir, revirar -->
  <rule weight="0.8">
    <match lemma="traduire" tags="vblex.*"><select lemma="traduire"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="traduire" tags="vblex.*"><select lemma="tradusir"/></match>
  </rule>

  <!-- tromper >> enganar, trompar... -->
  <rule weight="0.8">
    <match lemma="tromper" tags="vblex.*"><select lemma="enganar"/></match>
  </rule>

  <!-- conduire >> conduire/condusir, menar/miar -->
  <rule weight="0.8">
    <match lemma="conduire" tags="vblex.*"><select lemma="conduire"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="conduire" tags="vblex.*"><select lemma="condusir"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="conduire" tags="vblex.*"><select lemma="menar"/></match>
      <match lemma="conduire" tags="vblex.*"><select lemma="miar"/></match>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="det.*"/>
        <match tags="num.*"/>
        <match tags="num"/>
        <match tags="adj.*"/>
        <match lemma="de" tags="pr"/>
      </or>
    </repeat>
    <or>  
      <match lemma="véhicule" tags="n.*"/>
      <match lemma="voiture" tags="n.*"/>
      <match lemma="auto" tags="n.*"/>
      <match lemma="automobile" tags="n.*"/>
      <match lemma="camion" tags="n.*"/>
      <match lemma="car" tags="n.*"/>
      <match lemma="autocar" tags="n.*"/>
      <match lemma="bus" tags="n.*"/>
      <match lemma="autobus" tags="n.*"/>
      <match lemma="moto" tags="n.*"/>
      <match lemma="motocyclette" tags="n.*"/>
      <match lemma="char" tags="n.*"/>
      <match lemma="charrette" tags="n.*"/>
    </or> 
  </rule>
  <rule weight="1.0">
    <or>  
      <match lemma="véhicule" tags="n.*"/>
      <match lemma="voiture" tags="n.*"/>
      <match lemma="auto" tags="n.*"/>
      <match lemma="automobile" tags="n.*"/>
      <match lemma="camion" tags="n.*"/>
      <match lemma="car" tags="n.*"/>
      <match lemma="bus" tags="n.*"/>
      <match lemma="autobus" tags="n.*"/>
      <match lemma="moto" tags="n.*"/>
      <match lemma="motocyclette" tags="n.*"/>
      <match lemma="char" tags="n.*"/>
      <match lemma="charrette" tags="n.*"/>
    </or> 
    <repeat from="0" upto="4">
      <or>
        <match tags="cm"/>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="adj.*"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat>
    <or>
      <match lemma="conduire" tags="vblex.*"><select lemma="menar"/></match>
      <match lemma="conduire" tags="vblex.*"><select lemma="miar"/></match>
    </or>
  </rule>

  <!-- restar >> restar, demorar -->
  <rule weight="0.8">
    <match lemma="restar" tags="vblex.*"><select lemma="restar"/></match>
  </rule>
  <rule weight="0.9">
    <match lemma="restar" tags="vblex.*"><select lemma="demorar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <or>
      <match lemma="à" tags="pr"/>
      <match lemma="chez" tags="pr"/>
      <match lemma="ici" tags="adv"/>
      <match lemma="là" tags="adv"/>
      <match lemma="là-bas" tags="adv"/>
    </or>
  </rule>
  <rule weight="0.9">
    <match lemma="restar" tags="vblex.*"><select lemma="demorar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="en" tags="pr"/>
    <or>
      <match tags="np.top.*"/>
      <match tags=""/>
    </or>
  </rule>

  <!-- saisir >> arrapar, etc. -->
  <rule weight="0.8">
    <match lemma="saisir" tags="vblex.*"><select lemma="arrapar"/></match>
  </rule>

  <!-- apeler >> apelar, sonar, cridar... -->
  <rule weight="0.8">
    <match lemma="apeler" tags="vblex.*"><select lemma="apelar"/></match>
  </rule>

  <!-- tourner >> virar, rodar -->
  <rule weight="0.8">
    <match lemma="tourner" tags="vblex.*"><select lemma="virar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tourner" tags="vblex.*"><select lemma="rodar"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
    <match lemma="film" tags="n.*"/>
  </rule>

  <!-- enfonser >> enfonzar/en·honsar, etc -->
  <rule weight="0.8">
    <match lemma="enfonser" tags="vblex.*"><select lemma="enfonzar"/></match>
  </rule>
  <rule weight="0.9">
    <match lemma="enfonser" tags="vblex.*"><select lemma="en·honsar"/></match>
  </rule>

  <!-- couler >> rajar/colar, s'enfonzar/colar, enfonzar/ahonar -->
  <rule weight="0.8">
    <match lemma="couler" tags="vblex.*"><select lemma="rajar"/></match>
  </rule>
  <rule weight="0.9">
    <match lemma="couler" tags="vblex.*"><select lemma="colar"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="navire"/>
    </or>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="couler" tags="vblex.*"><select lemma="enfonzar" tags="vblex.pron.*"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="navire"/>
    </or>
    <match tags="vbhaver.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="couler" tags="vblex.*"><select lemma="enfonzar" tags="vblex.pron.*"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <seq n="navire"/>
    </or>
    <match tags="adv"/>
    <match tags="vbhaver.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="couler" tags="vblex.*"><select lemma="enfonzar" tags="vblex.pron.*"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="couler" tags="vblex.*"><select lemma="enfonzar" tags="vblex.nopron.*"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="couler" tags="vblex.*"><select lemma="enfonzar" tags="vblex.nopron.*"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="par" tags="pr"/>
  </rule>
  <rule weight="1.2">
    <match lemma="couler" tags="vblex.*"><select lemma="ahonar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.2">
    <match lemma="couler" tags="vblex.*"><select lemma="ahonar"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="par" tags="pr"/>
  </rule>

  <!-- décoller >> despegar, enlairar -->
  <macro name="verb_intrans_trans" p1="décoller" p2="enlairar" p3="despegar" />

  <!-- appeler >> cridar, sonar -->
  <macro name="verb_nopron_pron" p1="appeler" p2="cridar" p3="sonar" />

  <!-- souvenir >> recordar, rebrembar -->
  <macro name="verb_nopron_pron" p1="souvenir" p2="recordar" p3="rebrembar" />

  <!-- voler >> volar / raubar -->
  <rule weight="0.8">
    <match lemma="voler" tags="vblex.*"><select lemma="volar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.pp.*"><select lemma="raubar"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.*"/>
    <match lemma="voler" tags="vblex.pp.*"><select lemma="raubar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <match tags="adv"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="de" tags="pr"/>
    <or>
      <match tags="n.*.pl"/>
      <match tags="adj.*.pl"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="prn.tn.*"/>
  </rule>


