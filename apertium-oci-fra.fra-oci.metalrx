  </rule>
  <rule weight="0.8">
    <match lemma="chez elle" tags="adv" v="oci"><select lemma="en çò sieu"/></match>
    <match lemma="chez elle" tags="adv" v="oci@gascon"><select lemma="a lor"/></match>
