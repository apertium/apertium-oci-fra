<!-- **************** ADV **************** -->

  <!-- pas >> pas -->
  <rule weight="0.8">
    <match lemma="pas" tags="adv"><select lemma="pas"/></match>
  </rule>

  <!-- chez moi, toi, lui... après préposition -->
  <rule weight="0.8">
    <match lemma="chez moi" tags="adv" v="oci"><select lemma="en çò mieu"/></match>
    <match lemma="chez moi" tags="adv" v="oci@gascon"><select lemma="a noste"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="chez toi" tags="adv" v="oci"><select lemma="en çò tieu"/></match>
    <match lemma="chez toi" tags="adv" v="oci@gascon"><select lemma="a vòste"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="chez lui" tags="adv" v="oci"><select lemma="en çò sieu"/></match>
