<rules>


<!-- **************** PREP **************** -->

  <!-- vis-à-vis de >> de cap a, au respècte de -->
  <rule weight="0.6">
    <match lemma="vis-a-vis de" tags="pr" v="oci"><select lemma="de cap a"/></match>
    <match lemma="vis-a-vis de" tags="pr" v="oci@gascon"><select lemma="au respècte de"/></match>
  </rule>

  <!-- à >> a, en, de -->
  <rule weight="0.6">
    <match lemma="à" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="comencer" tags="vblex.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="*.inf"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="adv"/>
        <match tags="prn.pro.*"/>
      </or>
    </repeat>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.0">
    <match lemma="moulin" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
