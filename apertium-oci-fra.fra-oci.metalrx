<rules>


<!-- **************** PREP **************** -->

  <!-- en >> en, de -->
  <rule weight="0.8">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="de"/></match>
    <match lemma="biais" tags="n.*"/>
  </rule>

  <!-- pour >> per/entà -->
  <rule weight="0.8">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="0.9">
    <match lemma="pour" tags="pr"><select lemma="entà"/></match>
  </rule>

  <!-- lors de >> durant, pendent -->
  <rule weight="0.8">
    <match lemma="lors de" tags="pr"><select lemma="durant"/></match>
  </rule>


<!-- **************** REL **************** -->

