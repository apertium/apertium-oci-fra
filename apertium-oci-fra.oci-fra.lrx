    <!-- p1 = verb oci, p2 = verb fra no pron, p3 = verb fra amb pronom ci (li vaga) -->
    <def-macro n="verb_nopron_pronci" npar="3">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="prn.pro.p1.mf.sg"/>
          <match tags="prn.pro.p2.mf.sg"/>
          <match tags="prn.pro.p3.mf.sg" v="oci"/>
          <match tags="prn.pro.p3.m.sg" v="oci@gascon"><select lemma="lui"/></match>
          <match tags="prn.pro.p3.f.sg" v="oci@gascon"><select lemma="lui"/></match>
          <match tags="prn.pro.p1.mf.pl"/>
          <match tags="prn.pro.p2.mf.pl"/>
          <match tags="prn.pro.p3.mf.pl" v="oci"/>
          <match tags="prn.pro.p3.m.pl" v="oci@gascon"><select lemma="leur"/></match>
          <match tags="prn.pro.p3.f.pl" v="oci@gascon"><select lemma="leur"/></match>
        </or>
        <or>
          <match plemma="1" tags="vblex.*.p3.sg"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.*.p3.pl"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.inf"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.ger"><select plemma="3"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="prn.pro.p1.mf.sg"/>
          <match tags="prn.pro.p2.mf.sg"/>
          <match tags="prn.pro.p3.mf.sg" v="oci"/>
          <match tags="prn.pro.p3.m.sg" v="oci@gascon"><select lemma="lui"/></match>
