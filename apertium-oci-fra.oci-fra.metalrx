      <match lemma="devèrs" tags="pr"/>
      <match lemma="en" tags="pr"/>
      <match lemma="fòrça" tags="preadv"/>
      <match lemma="fòrça" tags="adv"/>
      <match lemma="hèra" tags="preadv"/>
      <match lemma="hèra" tags="adv"/>
      <match lemma="fòrt" tags="preadv"/>
      <match lemma="fòrt" tags="adv"/>
      <match lemma="hòrt" tags="preadv"/>
