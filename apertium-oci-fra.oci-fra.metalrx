<?xml version="1.0" encoding="UTF-8"?>
<metalrx>

  <def-macros>
    <def-macro name="noms_propres">
      <rule weight="0.6">
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p1}}"/></match>
      </rule>
      <rule weight="2.0">
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
        <match lemma="de" tags="pr"/>
        <or><match tags="np.loc"/><match tags="np.top.*"/></or>
      </rule>
      <rule weight="2.0">
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
        <match tags="det.*"/>
        <match tags="adj.*"/>
      </rule>
      <rule weight="2.0">
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
        <or>
          <match tags="num.*"/>
          <match lemma="I" tags="adj.*"/>
          <match lemma="II" tags="adj.*"/>
          <match lemma="III" tags="adj.*"/>
          <match lemma="IV" tags="adj.*"/>
          <match lemma="V" tags="adj.*"/>
          <match lemma="VI" tags="adj.*"/>
          <match lemma="VII" tags="adj.*"/>
          <match lemma="VIII" tags="adj.*"/>
          <match lemma="IX" tags="adj.*"/>
          <match lemma="X" tags="adj.*"/>
          <match lemma="XI" tags="adj.*"/>
          <match lemma="XII" tags="adj.*"/>
          <match lemma="XIII" tags="adj.*"/>
          <match lemma="XIV" tags="adj.*"/>
          <match lemma="XV" tags="adj.*"/>
          <match lemma="XVI" tags="adj.*"/>
          <match lemma="XVII" tags="adj.*"/>
          <match lemma="XVIII" tags="adj.*"/>
          <match lemma="XIX" tags="adj.*"/>
          <match lemma="XX" tags="adj.*"/>
          <match lemma="XXI" tags="adj.*"/>
          <match lemma="XXII" tags="adj.*"/>
          <match lemma="XXIII" tags="adj.*"/>
          <match lemma="XXIV" tags="adj.*"/>
          <match lemma="XXV" tags="adj.*"/>
          <match lemma="XXVI" tags="adj.*"/>
          <match lemma="XXVII" tags="adj.*"/>
          <match lemma="XXVIII" tags="adj.*"/>
          <match lemma="XXIX" tags="adj.*"/>
          <match lemma="XXX" tags="adj.*"/>
        </or>
      </rule>
      <rule weight="2.0">
        <or>
          <match lemma="papa" tags="n.*"/>
          <match lemma="avesque" tags="n.*"/>
          <match lemma="arquevesque" tags="n.*"/>
          <match lemma="sant" tags="n.*"/>
          <match lemma="sant" tags="adj.*"/>
          <match lemma="apostòl" tags="n.*"/>
          <match lemma="emperador" tags="n.*"/>
          <match lemma="emperaire" tags="n.*"/>
          <match lemma="rei" tags="n.*"/>
          <match lemma="tsar" tags="n.*"/>
          <match lemma="vice-roi" tags="n.*"/>
          <match lemma="prince" tags="n.*"/>
          <match lemma="infant" tags="n.*"/>
          <match lemma="dalfin" tags="n.*"/>
          <match lemma="comte" tags="n.*"/>
          <match lemma="vescomte" tags="n.*"/>
          <match lemma="duc" tags="n.*"/>
          <match lemma="duquessa" tags="n.*"/>
          <match lemma="archiduc" tags="n.*"/>
          <match lemma="archiduchesse" tags="n.*"/>
          <match lemma="marqués" tags="n.*"/>
          <match lemma="baron" tags="n.*"/>
        </or>
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="2.0">
        <or>
          <match lemma="papat" tags="n.*"/>
          <match lemma="pontificat" tags="n.*"/>
          <match lemma="empèri" tags="n.*"/>
          <match lemma="règne" tags="n.*"/>
          <match lemma="regéncia" tags="n.*"/>
          <match lemma="vice-royauté" tags="n.*"/>
        </or>
        <match lemma="de" tags="pr"/>
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="2.0">
        <match lemma="{{p1}}" tags="np.*"><select lemma="{{p2}}"/></match>
        <or>
          <match lemma="regnar" tags="vblex.*"/>
          <match lemma="governar" tags="vblex.*"/>
        </or>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 per traduir el prenom p1, però manté p1 si va seguit de cognom (o paraula desconeguda) -->
    <def-macro name="ant_antcog">
      <rule weight="0.6">
         <match lemma="{{p1}}" tags="np.ant.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="2.0">
         <match lemma="{{p1}}" tags="np.ant.*"><select lemma="{{p1}}"/></match>
         <or>
           <match tags="np.*"/>
           <match tags=""/>
         </or>
      </rule>
    </def-macro>

    <!-- p1 = verb oci, p2 = verb fra no pron, p3 = verb fra pron -->
    <def-macro name="verb_nopron_pron">
      <rule weight="0.8">
        <match lemma="{{p1}}" tags="vblex.*"><select lemma="{{p2}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.sg"/>
        <match lemma="{{p1}}" tags="vblex.*.p1.sg"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.sg"/>
        <match lemma="{{p1}}" tags="vblex.*.p2.sg"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.ref.p3.*.sp"/>
        <or>
          <match lemma="{{p1}}" tags="vblex.*.p3.sg"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.*.p3.pl"><select lemma="{{p3}}"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.pl"/>
        <match lemma="{{p1}}" tags="vblex.*.p1.pl"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.pl"/>
        <match lemma="{{p1}}" tags="vblex.*.p2.pl"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p3.*"/>
        <match lemma="{{p1}}" tags="vblex.*.p3.*"><select lemma="{{p3}}"/></match>
        <repeat from="0" upto="1">
          <or>
            <match tags="adv"/>
            <match tags="preadv"/>
          </or>
        </repeat>
        <or>
          <match tags="np.ant"/>
          <match tags="np.ant.*"/>
          <match tags="np.cog.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <or>
          <match lemma="aller" tags="vblex.*.p3.sg"/>
          <match lemma="aller" tags="vblex.*.p3.pl"/>
        </or>
        <repeat from="0" upto="1">
          <or>
            <match tags="adv"/>
            <match tags="preadv"/>
          </or>
        </repeat>
        <match tags="prn.pro.ref.p3.*.sp"/>
        <or>
          <match lemma="{{p1}}" tags="vblex.inf"><select lemma="{{p3}}"/></match>
          <match lemma="{{p1}}" tags="vblex.tv.inf"><select lemma="{{p3}}"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <match tags="vbser.*"/>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="n.*"/>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="cm"/>
          <match tags="lpar"/>
        </or>
        <match lemma="{{p1}}" tags="vblex.pp.*"><select lemma="{{p3}}"/></match>
      </rule>
    </def-macro>

  </def-macros>

  <def-seqs>
    <def-seq n="temps">
      <match lemma="segonda" tags="n.*"/>
      <match lemma="minuta" tags="n.*"/>
      <match lemma="ora" tags="n.*"/>
      <match lemma="jorn" tags="n.*"/>
      <match lemma="setmana" tags="n.*"/>
      <match lemma="mes" tags="n.*"/>
      <match lemma="annada" tags="n.*"/>
      <match lemma="lustre" tags="n.*"/>
      <match lemma="decada" tags="n.*"/>
      <match lemma="sègle" tags="n.*"/>
      <match lemma="millenari" tags="n.*"/>
    </def-seq>

    <def-seq n="locucion_ini_a">
      <match lemma="a la mitat de genièr" tags="adv"/>
      <match lemma="a la mieitat de genèr" tags="adv"/>
      <match lemma="a la mitat de febrièr" tags="adv"/>
      <match lemma="a la mieitat de heurèr" tags="adv"/>
      <match lemma="a la mitat de març" tags="adv"/>
      <match lemma="a la mieitat de març" tags="adv"/>
      <match lemma="a la mitat d'abril" tags="adv"/>
      <match lemma="a la mitat d'abrial" tags="adv"/>
      <match lemma="a la mieitat d'abriu" tags="adv"/>
      <match lemma="a la mitat de mai" tags="adv"/>
      <match lemma="a la mieitat de mai" tags="adv"/>
      <match lemma="a la mitat de junh" tags="adv"/>
      <match lemma="a la mieitat de junh" tags="adv"/>
      <match lemma="a la mitat de julhet" tags="adv"/>
      <match lemma="a la mieitat de julhet" tags="adv"/>
      <match lemma="a la mitat de julh" tags="adv"/>
      <match lemma="a la mieitat d'agost" tags="adv"/>
      <match lemma="a la mitat de setembre" tags="adv"/>
      <match lemma="a la mieitat de seteme" tags="adv"/>
      <match lemma="a la mitat d'octobre" tags="adv"/>
      <match lemma="a la mieitat d'octobre" tags="adv"/>
      <match lemma="a la mitat de novembre" tags="adv"/>
      <match lemma="a la mieitat de noveme" tags="adv"/>
      <match lemma="a la mitat de decembre" tags="adv"/>
      <match lemma="a la mieitat de deceme" tags="adv"/>
    </def-seq>
  </def-seqs>

<rules>

<!-- **************** PRN **************** -->

  <!-- aquò >> cela, ce -->
  <rule weight="0.8">
    <match lemma="aquò" tags="prn.*"><select lemma="cela"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="aquò" tags="prn.*"><select lemma="ce"/></match>
    <match tags="vbser.*.p3.*"/>
  </rule>
  <rule weight="1.2">
    <match tags="pr"/>
    <match lemma="aquò" tags="prn.*"><select lemma="cela"/></match>
    <match tags="vbser.*.p3.*"/>
  </rule>

  <!-- çò >> cela, ce -->
  <rule weight="0.8">
    <match lemma="çò" tags="prn.*"><select lemma="cela"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="çò" tags="prn.*"><select lemma="ce"/></match>
    <match tags="vbser.*.p3.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="çò" tags="prn.*"><select lemma="ce"/></match>
    <match lemma="que"/>
  </rule>
  <rule weight="1.0">
    <match lemma="çò" tags="prn.*"><select lemma="ce"/></match>
    <match lemma="qui"/>
  </rule>

  <!-- ieu/jo >> je, moi -->
  <rule weight="0.8">
    <or>
      <match lemma="ieu" tags="prn.*"><select lemma="je"/></match>
      <match lemma="jo" tags="prn.*"><select lemma="je"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="pr"/>
    <or>
      <match lemma="ieu" tags="prn.*"><select lemma="moi"/></match>
      <match lemma="jo" tags="prn.*"><select lemma="moi"/></match>
    </or>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="ieu" tags="prn.*"><select lemma="moi"/></match>
      <match lemma="jo" tags="prn.*"><select lemma="moi"/></match>
    </or>
    <match tags="cm"/>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="ieu" tags="prn.*"><select lemma="moi"/></match>
      <match lemma="jo" tags="prn.*"><select lemma="moi"/></match>
    </or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match tags="cm"/>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="ieu" tags="prn.*"><select lemma="moi"/></match>
      <match lemma="jo" tags="prn.*"><select lemma="moi"/></match>
    </or>
    <or>
      <match lemma="atanben" tags="adv"/>
      <match lemma="tanben" tags="adv"/>
      <match lemma="tanplan" tags="adv"/>
      <match lemma="tanpauc" tags="adv"/>
      <match lemma="aitanpauc" tags="adv"/>
      <match lemma="atanpauc" tags="adv"/>
      <match lemma="tanpòc" tags="adv"/>
      <match lemma="tampòc" tags="adv"/>
    </or>
  </rule>

  <!-- tu >> tu, toi -->
  <rule weight="0.8">
    <match lemma="tu" tags="prn.*"><select lemma="tu"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="pr"/>
    <match lemma="tu" tags="prn.*"><select lemma="toi"/></match>
  </rule>
  <rule weight="0.9">
    <match lemma="tu" tags="prn.*"><select lemma="toi"/></match>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match tags="cm"/>
  </rule>
  <rule weight="0.9">
    <match lemma="tu" tags="prn.*"><select lemma="toi"/></match>
    <or>
      <match lemma="atanben" tags="adv"/>
      <match lemma="tanben" tags="adv"/>
      <match lemma="tanplan" tags="adv"/>
      <match lemma="tanpauc" tags="adv"/>
      <match lemma="aitanpauc" tags="adv"/>
      <match lemma="atanpauc" tags="adv"/>
      <match lemma="tanpòc" tags="adv"/>
      <match lemma="tampòc" tags="adv"/>
    </or>
  </rule>

  <!-- el/eth >> il(s), lui, eux -->
  <rule weight="0.8">
    <or>
      <match lemma="el" tags="prn.*"><select lemma="il"/></match>
      <match lemma="prneth" tags="prn.*"><select lemma="il"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="pr"/>
    <or>
      <match lemma="el" tags="prn.tn.p3.m.sg"><select lemma="lui"/></match>
      <match lemma="prneth" tags="prn.tn.p3.m.sg"><select lemma="lui"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="pr"/>
    <or>
      <match lemma="el" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
      <match lemma="prneth" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
    </or>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="el" tags="prn.tn.p3.m.sg"><select lemma="lui"/></match>
      <match lemma="prneth" tags="prn.tn.p3.m.sg"><select lemma="lui"/></match>
    </or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match tags="cm"/>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="el" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
      <match lemma="prneth" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
    </or>
    <repeat from="0" upto="1">
      <or>
        <match tags="adv"/>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match tags="cm"/>
  </rule>
  <rule weight="0.9">
    <or>
      <match lemma="el" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
      <match lemma="prneth" tags="prn.tn.p3.m.pl"><select lemma="eux"/></match>
    </or>
    <or>
      <match lemma="atanben" tags="adv"/>
      <match lemma="tanben" tags="adv"/>
      <match lemma="tanplan" tags="adv"/>
      <match lemma="tanpauc" tags="adv"/>
      <match lemma="aitanpauc" tags="adv"/>
      <match lemma="atanpauc" tags="adv"/>
      <match lemma="tanpòc" tags="adv"/>
      <match lemma="tampòc" tags="adv"/>
    </or>
  </rule>

  <!-- lo >> le, lui -->
  <rule weight="0.8">
    <match lemma="lo" tags="prn.pro.*"><select lemma="le"/></match>
  </rule>

<!-- **************** PREP **************** -->

  <!-- pendent >> pendant, lors de -->
  <rule weight="0.8">
    <match lemma="pendent" tags="pr"><select lemma="pendant"/></match>
  </rule>

  <!-- denquia >> jusqu'à, jusque -->
  <rule weight="0.8">
    <match lemma="denquia" tags="pr"><select lemma="jusqu'à"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="denquia" tags="pr"><select lemma="jusque"/></match>
    <or>
      <match lemma="a" tags="pr"/>
      <match lemma="en cò de" tags="pr"/>
      <match lemma="en çò de" tags="pr"/>
      <match lemma="a çò de" tags="pr"/>
      <match lemma="cò de" tags="pr"/>
      <match lemma="chas" tags="pr"/>
      <match lemma="dins" tags="pr"/>
      <match lemma="dens" tags="pr"/>
      <match lemma="cap a" tags="pr"/>
      <match lemma="devèrs" tags="pr"/>
      <match lemma="en" tags="pr"/>
      <match lemma="fòrça" tags="preadv"/>
      <match lemma="fòrça" tags="adv"/>
      <match lemma="hèra" tags="preadv"/>
      <match lemma="hèra" tags="adv"/>
      <match lemma="fòrt" tags="preadv"/>
      <match lemma="fòrt" tags="adv"/>
      <match lemma="hòrt" tags="preadv"/>
      <match lemma="hòrt" tags="adv"/>
      <match lemma="plan" tags="preadv"/>
      <match lemma="plan" tags="adv"/>
      <seq n="locucion_ini_a"/>
    </or>
  </rule>

  <!-- dinc >> jusqu'à, jusque -->
