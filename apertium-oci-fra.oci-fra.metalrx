      <match lemma="menar" tags="vblex.*"><select lemma="conduire"/></match>
      <match lemma="miar" tags="vblex.*"><select lemma="conduire"/></match>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="det.*"/>
        <match tags="num.*"/>
        <match tags="num"/>
        <match tags="adj.*"/>
        <match lemma="de" tags="pr"/>
      </or>
    </repeat>
    <or>
      <match lemma="veïcule" tags="n.*"/>
      <match lemma="veitura" tags="n.*"/>
      <match lemma="auto" tags="n.*"/>
      <match lemma="automobila" tags="n.*"/>
      <match lemma="camion" tags="n.*"/>
      <match lemma="car" tags="n.*"/>
      <match lemma="autocar" tags="n.*"/>
      <match lemma="autocarri" tags="n.*"/>
      <match lemma="bus" tags="n.*"/>
      <match lemma="autobús" tags="n.*"/>
      <match lemma="autobus" tags="n.*"/>
      <match lemma="mòto" tags="n.*"/>
      <match lemma="moto" tags="n.*"/>
      <match lemma="motocicleta" tags="n.*"/>
      <match lemma="carri" tags="n.*"/>
      <match lemma="carro" tags="n.*"/>
      <match lemma="motocicleta" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="veïcule" tags="n.*"/>
      <match lemma="veitura" tags="n.*"/>
      <match lemma="auto" tags="n.*"/>
      <match lemma="automobila" tags="n.*"/>
      <match lemma="camion" tags="n.*"/>
      <match lemma="car" tags="n.*"/>
      <match lemma="autocar" tags="n.*"/>
      <match lemma="autocarri" tags="n.*"/>
      <match lemma="bus" tags="n.*"/>
      <match lemma="autobús" tags="n.*"/>
      <match lemma="autobus" tags="n.*"/>
      <match lemma="mòto" tags="n.*"/>
      <match lemma="moto" tags="n.*"/>
      <match lemma="motocicleta" tags="n.*"/>
      <match lemma="carri" tags="n.*"/>
      <match lemma="carro" tags="n.*"/>
      <match lemma="motocicleta" tags="n.*"/>
    </or>
    <repeat from="0" upto="4">
      <or>
        <match tags="cm"/>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="adj.*"/>
        <match tags="cnjcoo"/>
      </or>
    </repeat>
    <or>
      <match lemma="menar" tags="vblex.*"><select lemma="conduire"/></match>
      <match lemma="miar" tags="vblex.*"><select lemma="conduire"/></match>
    </or>
  </rule>

  <!-- velhar >> veiller, voiler -->
  <rule weight="0.8">
    <match lemma="velhar" tags="vblex.*"><select lemma="veiller"/></match>
  </rule>

  <!-- crochetar >> accrocher, atteler -->
  <rule weight="0.8">
    <match lemma="crochetar" tags="vblex.*"><select lemma="accrocher"/></match>
  </rule>

  <!-- tornar >> revenir, rentrer -->
  <rule weight="0.8">
    <match lemma="tornar" tags="vblex.*"><select lemma="revenir"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="lo" tags="prn.pro.*"/>
      <match lemma="o" tags="prn.pro.*"/>
      <match lemma="ac" tags="prn.pro.*"/>
    </or>
    <match lemma="tornar" tags="vblex.*"><select lemma="rendre"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tornar" tags="vblex.*"><select lemma="rendre"/></match>
    <or>
      <match lemma="lo" tags="prn.enc.*"/>
      <match lemma="o" tags="prn.enc.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="tornar" tags="vblex.*"><select lemma="rendre"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="prn.enc.*"/>
      </or>
    </repeat>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.2">
    <or>
      <match lemma="me" tags="prn.pro.*"/>
      <match lemma="te" tags="prn.pro.*"/>
      <match lemma="se" tags="prn.pro.*"/>
      <match lemma="nos" tags="prn.pro.*"/>
      <match lemma="vos" tags="prn.pro.*"/>
    </or>
    <match lemma="en" tags="prn.pro.*"/>
    <match lemma="tornar" tags="vblex.*"><select lemma="aller"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tornar" tags="vblex.*"><select lemma="aller"/></match>
    <or>
      <match lemma="me" tags="prn.enc.*"/>
      <match lemma="te" tags="prn.enc.*"/>
      <match lemma="se" tags="prn.enc.*"/>
      <match lemma="nos" tags="prn.enc.*"/>
      <match lemma="vos" tags="prn.enc.*"/>
    </or>
    <match lemma="en" tags="prn.enc.*"/>
  </rule>

  <!-- cridar >> appeler, crier -->
  <rule weight="0.8">
    <match lemma="cridar" tags="vblex.*"><select lemma="appeler"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="a" tags="pr"/>
  </rule>
  <rule weight="1.2">
    <match lemma="cridar" tags="vblex.*"><select lemma="appeler"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="pr"/>
    <or><match tags="vblex.inf"/><match tags="vblex.tv.inf"/><match tags="vbser.inf"/><match tags="vbhaver.inf"/><match tags="vbmod.inf"/></or>
  </rule>
  <rule weight="1.2">
    <match lemma="cridar" tags="vblex.*"><select lemma="appeler"/></match>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="pr"/>
    <match tags="prn.pro.*"/>
    <or><match tags="vblex.inf"/><match tags="vblex.tv.inf"/><match tags="vbser.inf"/><match tags="vbhaver.inf"/><match tags="vbmod.inf"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="li" tags="prn.pro.*"/>
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cridar" tags="vblex.*"><select lemma="crier"/></match>
    <match lemma="li" tags="prn.enc.*"/>
  </rule>

  <!-- venir >> venir, devenir -->
  <rule weight="0.8">
    <match lemma="venir" tags="vblex.*"><select lemma="venir"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="venir" tags="vblex.*"><select lemma="devenir"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <or>
      <match tags="n.*"/>
      <match tags="adj.*"/>
    </or>
  </rule>

  <!-- mesclar >> mélanger, mêler -->
  <macro name="verb_nopron_pron" p1="mesclar" p2="mélanger" p3="mêler" />

