  <!-- entrò >> jusqu'à, jusque -->
  <rule weight="0.8">
    <match lemma="entrò" tags="pr"><select lemma="jusqu'à"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="entrò" tags="pr"><select lemma="jusque"/></match>
