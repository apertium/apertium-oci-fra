  <!-- accoucher >> amainadar, har# amainadar -->
  <macro n="verb_intrans_trans" v="oci"><with-param v="accoucher"/><with-param v="ajaire"/><with-param v="levar"/></macro>
  <macro n="verb_intrans_trans" v="oci@gascon"><with-param v="accoucher"/><with-param v="amainadar"/><with-param v="har# amainadar"/></macro>

  <!-- décoller >> despegar, enlairar -->
  <macro n="verb_intrans_trans"><with-param v="décoller"/><with-param v="enlairar"/><with-param v="despegar"/></macro>

  <!-- déplafonner >> arrestar# la limitacion, arrestar# la limitacion de -->
  <macro n="verb_intrans_trans"><with-param v="déplafonner"/><with-param v="arrestar# la limitacion"/><with-param v="arrestar# la limitacion de"/></macro>

  <!-- enrager >> s'enrabiar, enrabiar -->
  <!-- enrager >> amainadar, har# amainadar -->
  <macro n="verb_intrans_trans-pron_nopron" v="oci"><with-param v="enrager"/><with-param v="enrabiar"/></macro>
<!--
  <macro n="verb_intrans_trans" v="oci@gascon"><with-param v="enrager"/><with-param v="amainadar"/><with-param v="har# amainadar"/></macro>
-->

  <!-- gronder >> bronzir, cridar -->
  <macro n="verb_intrans_trans"><with-param v="gronder"/><with-param v="bronzir"/><with-param v="cridar"/></macro>
  <rule weight="1.2">
    <or>
      <seq n="personne"/>
      <seq n="parent"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <repeat from="0" upto="2">
      <match tags="prn.pro.*"/>
    </repeat>
    <match lemma="gronder" tags="vblex.*"><select lemma="cridar"/></match>
  </rule>

  <!-- virer >> virar, botar# dehòra -->
  <macro n="verb_intrans_trans" v="oci"><with-param v="virer"/><with-param v="virar"/><with-param v="botar# defòra"/></macro>
  <macro n="verb_intrans_trans" v="oci@gascon"><with-param v="virer"/><with-param v="virar"/><with-param v="botar# dehòra"/></macro>

  <!-- mêler >> mesclar, mainar/aucupar -->
  <macro n="verb_nopron_pron_prep" v="oci"><with-param v="mêler"/><with-param v="mesclar"/><with-param v="mainar"/><with-param v="de"/></macro>
  <macro n="verb_nopron_pron_prep" v="oci@gascon"><with-param v="mêler"/><with-param v="mesclar"/><with-param v="aucupar"/><with-param v="de"/></macro>

  <!-- souvenir >> recordar, rebrembar -->
  <macro n="verb_nopron_pron"><with-param v="souvenir"/><with-param v="recordar"/><with-param v="rebrembar"/></macro>

  <!-- décortiquer -> descloscar, derruscar, destecar -->
  <rule weight="0.8">
    <match lemma="décortiquer" tags="vblex.*"><select lemma="descloscar"/></match>
  </rule>

  <!-- voler >> volar / raubar -->
  <rule weight="0.8">
    <match lemma="voler" tags="vblex.*"><select lemma="volar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.pp.*"><select lemma="raubar"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="prn.pro.*"/>
    <match lemma="voler" tags="vblex.pp.*"><select lemma="raubar"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <match tags="adv"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match lemma="de" tags="pr"/>
    <or>
      <match tags="n.*.pl"/>
      <match tags="adj.*.pl"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="voler" tags="vblex.*"><select lemma="raubar"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="cnjadv"/>
      </or>
    </repeat>
    <match tags="prn.tn.*"/>
  </rule>


  <!-- distraire >> divertir/destorbar -->
  <rule weight="0.8">
    <match lemma="distraire" tags="vblex.*"><select lemma="destorbar"/></match>
  </rule>

  <macro n="verb_nopron_pron"><with-param v="distraire"/><with-param v="destorbar"/><with-param v="divertir"/></macro>


  <!-- foncer >> (ar)ronçar/escurcir -->
  <rule weight="0.8">
      <match lemma="foncer" tags="vblex.*"><select lemma="escurir"/></match>
  </rule>
  <rule weight="1.0">
      <match lemma="foncer" tags="vblex.*" ><select lemma="ronçar"/></match>
      <match lemma="sur" tags="pr"/>
  </rule>


  <!-- repérer >> localizar/senhalar -->
  <rule weight="0.8">
      <match lemma="repérer" tags="vblex.*"><select lemma="localizar"/></match>
  </rule>


  <!-- échouer >> mal escaire/(s'i) mauescàder, encalar/encalhar -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="échouer"/><with-param v="mal escaire"/><with-param v="encalar"/></macro>
  <macro n="verb_nopron_pron" v="oci@gascon"><with-param v="échouer"/><with-param v="i mauescàder"/><with-param v="encalhar"/></macro>
  <rule weight="0.9" v="oci@gascon">
    <match lemma="échouer" tags="vblex.pp.*"><select lemma="mauescàder"/></match>
  </rule>

  <!-- ébranler >> brandir, abranlir / susmàver, trebolar -->
  <rule weight="0.8">
      <match lemma="ébranler" tags="vblex.*" v="oci"><select lemma="brandir"/></match>
      <match lemma="ébranler" tags="vblex.*" v="oci@gascon"><select lemma="abranlir"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ébranler" tags="vblex.*" v="oci"><select lemma="trebolar"/></match>
    <match lemma="ébranler" tags="vblex.*" v="oci@gascon"><select lemma="susmàver"/></match>
    <repeat from="0" upto="3">
      <or>
       <match tags="det.*"/>
       <match tags="num.*"/>
       <match tags="num"/>
       <match tags="adv"/>
       <match tags="preadv"/>
       <match tags="adj.*"/>
      </or>
    </repeat>
    <or>
        <match lemma="opinion" tags="n.*"/>
        <match lemma="volontat" tags="n.*"/>
        <match lemma="determinacion" tags="n.*"/>
        <match lemma="coratge" tags="n.*"/>
        <match lemma="fisança" tags="n.*"/>
        <match lemma="hidança" tags="n.*"/>
        <match lemma="confisança" tags="n.*"/>
        <match lemma="hida" tags="n.*"/>
        <match lemma="ahida" tags="n.*"/>
        <match lemma="hisança" tags="n.*"/>
        <match lemma="certitud" tags="n.*"/>
        <match lemma="certesa" tags="n.*"/>
        <match lemma="credença" tags="n.*"/>
        <match lemma="cresença" tags="n.*"/>
        <match lemma="créder" tags="n.*"/>
        <match lemma="fe" tags="n.*"/>
        <match lemma="he" tags="n.*"/>
        <match lemma="prejutjat" tags="n.*"/>
        <match lemma="esperit" tags="n.*"/>
        <match lemma="pensada" tags="n.*"/>
        <match lemma="conviccion" tags="n.*"/>
        <match lemma="créser" tags="n.*"/>
        <match lemma="cregénça" tags="n.*"/>
        <match lemma="òrdre" tags="n.*"/>
        <match lemma="òrdi" tags="n.*"/>
        <match lemma="societat" tags="n.*"/>
        <match lemma="ordi" tags="n.*"/>
        <match lemma="òrde" tags="n.*"/>
        <match lemma="poder" tags="n.*"/>
        <match lemma="constància" tags="n.*"/>
        <match lemma="principi" tags="n.*"/>
    </or>
  </rule>

  <!-- recouper >> retalhar, concordar -->
  <macro n="verb_nopron_pron"><with-param v="recouper"/><with-param v="retalhar"/><with-param v="concordar"/></macro>

  <!-- découper >> destacar, copar -->
  <macro n="verb_nopron_pron"><with-param v="découper"/><with-param v="copar"/><with-param v="destacar"/></macro>

  <!-- rendre >> tornar, aller, far venir/har vàder, venir/vàder -->
  <rule weight="0.8">
      <match lemma="rendre" tags="vblex.*"><select lemma="tornar"/></match>
  </rule>
  <rule weight="1.0">
    <or>
        <match lemma="me" tags="prn.pro.*"/>
        <match lemma="te" tags="prn.pro.*"/>
        <match lemma="se" tags="prn.pro.*"/>
        <match lemma="nos" tags="prn.pro.*"/>
        <match lemma="vos" tags="prn.pro.*"/>
    </or>
    <repeat from="0" upto="3">
      <or>
       <match tags="vbser.*"/>
       <match tags="adv"/>
      </or>
    </repeat>
    <match lemma="rendre" tags="vblex.*"><select lemma="anar"/></match>
    <repeat from="0" upto="2">
     <match tags="adv"/>
    </repeat>
    <or>
        <match lemma="à" tags="pr"/>
        <match lemma="sur" tags="pr"/>
        <match lemma="vers" tags="pr"/>
        <match lemma="chez" tags="pr"/>
        <match lemma="dans" tags="pr"/>
        <match lemma="chez moi" tags="adv"/>
        <match lemma="chez toi" tags="adv"/>
        <match lemma="chez lui" tags="adv"/>
        <match lemma="chez elle" tags="adv"/>
        <match lemma="chez nous" tags="adv"/>
        <match lemma="chez vous" tags="adv"/>
        <match lemma="chez eux" tags="adv"/>
        <match lemma="chez elles" tags="adv"/>
        <match lemma="chez soi" tags="adv"/>
        <match lemma="à la maison" tags="adv"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
        <match lemma="me" tags="prn.pro.*"/>
        <match lemma="te" tags="prn.pro.*"/>
        <match lemma="se" tags="prn.pro.*"/>
        <match lemma="nos" tags="prn.pro.*"/>
        <match lemma="vos" tags="prn.pro.*"/>
    </or>
    <repeat from="0" upto="3">
      <or>
       <match tags="vbser.*"/>
       <match tags="adv"/>
      </or>
    </repeat>
    <match lemma="rendre" tags="vblex.*"><select lemma="anar"/></match>
    <match lemma="en" tags="pr"/>
    <or>
        <match tags="np.top.*"/>
        <match tags="" case="Aa"/>
    </or>
  </rule>

  <rule weight="1.2">
    <match lemma="rendre" tags="vblex.*" v="oci"><select lemma="rendre"/></match>
    <match lemma="rendre" tags="vblex.*" v="oci@gascon"><select lemma="har"/></match>
    <repeat from="0" upto="2">
      <or>
        <match lemma="plus" tags="adv"/>
        <match lemma="moins" tags="adv"/>
        <match lemma="aussi" tags="adv"/>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <or>
        <match tags="adj.*"/>
    </or>
  </rule>

  <rule weight="1.0">
    <match tags="prn.pro.*"/>
    <match lemma="rendre" tags="vblex.*" v="oci"><select lemma="venir"/></match>
    <match lemma="rendre" tags="vblex.*" v="oci@gascon"><select lemma="vàder"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="adj,*"/>
      </or>
    </repeat>
    <or>
        <match lemma="maître" tags="n.*"/>
    </or>
  </rule>

  <!-- rendre# compte >> rendre# compte, mainar -->
  <!-- rendre# compte >> rénder# compte, avisar -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="rendre# compte"/><with-param v="rendre# compte"/><with-param v="mainar"/></macro>
  <macro n="verb_nopron_pron" v="oci@gascon"><with-param v="rendre# compte"/><with-param v="rénder# compte"/><with-param v="avisar"/></macro>

  <!-- vantar >> vanter, louer -->
  <macro n="verb_nopron_pron"><with-param v="vantar"/><with-param v="louer"/><with-param v="vanter"/></macro>

  <!-- dépêcher, se dépêcher -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="dépêcher"/><with-param v="despachar"/><with-param v="trigar"/></macro>
  <macro n="verb_nopron_pron" v="oci@gascon"><with-param v="dépêcher"/><with-param v="despachar"/><with-param v="amanejar"/></macro>


  <!-- borner >> bornar, limitar -->
  <macro n="verb_nopron_pron"><with-param v="borner"/><with-param v="bornar"/><with-param v="limitar"/></macro>


  <!-- traîner >> longanhejar/longuejar, tirassar/arossegar, pron rebalar/arossegar -->
  <macro n="verb_intrans_trans_pron" v="oci"><with-param v="traîner"/><with-param v="longanhejar"/><with-param v="tirassar"/><with-param v="rebalar"/></macro>
  <macro n="verb_intrans_trans_pron" v="oci@gascon"><with-param v="traîner"/><with-param v="longuejar"/><with-param v="arossegar"/><with-param v="arossegar"/></macro>


  <!-- viser >> mirar, visar -->
  <rule weight="0.8">
      <match lemma="viser" tags="vblex.*"><select lemma="mirar"/></match>
  </rule>


  <!-- divaguer >> landrar/arrondejar, desparlar/chabraquejar -->
  <rule weight="0.8">
      <match lemma="divaguer" tags="vblex.*" v="oci"><select lemma="desparlar"/></match>
      <match lemma="divaguer" tags="vblex.*" v="oci@gascon"><select lemma="chabraquejar"/></match>
  </rule>


  <!-- fouler >> caucar/traupir, tòrcer/virar -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="fouler"/><with-param v="caucar"/><with-param v="tòrcer"/></macro>
  <macro n="verb_nopron_pron" v="oci@gascon"><with-param v="fouler"/><with-param v="traupir"/><with-param v="virar"/></macro>

  <!-- replier >> tornar plegar, replegar/aplegar -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="replier"/><with-param v="tornar# plegar"/><with-param v="replegar"/></macro>
  <macro n="verb_nopron_pron" v="oci@gascon"><with-param v="replier"/><with-param v="tornar# plegar"/><with-param v="aplegar"/></macro>

  <!-- ressaisir >> tornar agafar, dominar -->
  <macro n="verb_nopron_pron" v="oci"><with-param v="ressaisir"/><with-param v="tornar# agafar"/><with-param v="dominar"/></macro>

</rules>
</lrx>

