        <match plemma="1" tags="vblex.*.p2.sg"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*.p2.sg"><select plemma="3"/></match>
        <match tags="prn.enc.p2.*.sg"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.ref.p3.*.sp"/>
        <or>
          <match plemma="1" tags="vblex.*.p3.sg"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.*.p3.pl"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.inf"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.ger"><select plemma="3"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.ref.p3.*.sp"/>
        <match tags="prn.pro.*"/>
        <or>
          <match plemma="1" tags="vblex.*.p3.sg"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.*.p3.pl"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.inf"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.ger"><select plemma="3"/></match>
        </or>
      </rule>
      <rule weight="1.0">
        <or>
          <match plemma="1" tags="vblex.*.p3.sg"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.*.p3.pl"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.inf"><select plemma="3"/></match>
          <match plemma="1" tags="vblex.ger"><select plemma="3"/></match>
        </or>
        <match tags="prn.enc.ref.p3.*.sp"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.pl"/>
        <match plemma="1" tags="vblex.*.p1.pl"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p1.*.pl"/>
        <match tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*.p1.pl"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*.p1.pl"><select plemma="3"/></match>
        <match tags="prn.enc.p1.*.pl"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.pl"/>
        <match plemma="1" tags="vblex.*.p2.pl"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p2.*.pl"/>
        <match tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*.p2.pl"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*.p2.pl"><select plemma="3"/></match>
        <match tags="prn.enc.p2.*.pl"/>
      </rule>
      <rule weight="1.0">
        <match tags="prn.pro.p3.*"/>
        <match plemma="1" tags="vblex.*.p3.*"><select plemma="3"/></match>
        <repeat from="0" upto="1">
          <or>
            <match tags="adv"/>
            <match tags="preadv"/>
          </or>
        </repeat>
        <or>
          <match tags="np.ant"/>
          <match tags="np.ant.*"/>
          <match tags="np.cog.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match tags="vbser.*"/>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="n.*"/>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="cm"/>
          <match tags="lpar"/>
        </or>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 si p1 no és pronominal i p3 si p1 és pronominal i va seguit de la preposició p4 -->
    <!-- p1 = verb fra, p2 = verb oci no pron, p3 = verb oci pron, p4 = prep fra -->
    <def-macro n="verb_nopron_pron_prep" npar="4">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
