<?xml version="1.0" encoding="UTF-8"?>
<lrx>
  <def-macros>
    <!-- p1 verb fra, p2 verb oci estandard, p3 prep fra, p3 verb oci alternatiu -->
     <!-- <macro n="verb_prep"><with-param v="chasser"/><with-param v="caçar"/><with-param v="de"/><with-param v="expulsar"/></macro> -->
    <def-macro n="verb_prep" npar="4">
      <rule weight="0.6">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="4"/></match>
        <match plemma="3" tags="pr"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="4"/></match>
        <match tags="adv"/>
        <match plemma="3" tags="pr"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="4"/></match>
        <match tags="adv"/>
        <match tags="adv"/>
        <match plemma="3" tags="pr"/>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 si p1 és intransitiu i p3 si p1 és transitiu -->
    <!-- p1 = verb fra, p2 = verb oci intransitiu, p3 = verb oci transitiu -->
    <!-- la manera de veure que el verb és transitiu és molt barroera -->
    <def-macro n="verb_intrans_trans" npar="3">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.2">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match tags="det.*"/>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <or>
          <match lemma="jour" tags="n.*"/>
          <match lemma="journée" tags="n.*"/>
          <match lemma="veille" tags="n.*"/>
          <match lemma="veille" tags="n.*"/>
          <match lemma="nuit" tags="n.*"/>
          <match lemma="matin" tags="n.*"/>
          <match lemma="matinée" tags="n.*"/>
          <match lemma="après-midi" tags="n.*"/>
          <match lemma="soir" tags="n.*"/>
          <match lemma="soirée" tags="n.*"/>
          <match lemma="week-end" tags="n.*"/>
          <match lemma="weekend" tags="n.*"/>
          <match lemma="semaine" tags="n.*"/>
          <match lemma="mois" tags="n.*"/>
          <match lemma="année" tags="n.*"/>
          <match lemma="lustre" tags="n.*"/>
          <match lemma="décennie" tags="n.*"/>
          <match lemma="siècle" tags="n.*"/>
          <match lemma="vacances" tags="n.*"/>
        </or>
      </rule>
      <rule weight="1.4">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match tags="det.*"/>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <or>
          <match lemma="jour" tags="n.*"/>
          <match lemma="journée" tags="n.*"/>
          <match lemma="veille" tags="n.*"/>
          <match lemma="veille" tags="n.*"/>
          <match lemma="nuit" tags="n.*"/>
          <match lemma="matin" tags="n.*"/>
          <match lemma="matinée" tags="n.*"/>
          <match lemma="après-midi" tags="n.*"/>
          <match lemma="soir" tags="n.*"/>
          <match lemma="soirée" tags="n.*"/>
          <match lemma="week-end" tags="n.*"/>
          <match lemma="weekend" tags="n.*"/>
          <match lemma="semaine" tags="n.*"/>
          <match lemma="mois" tags="n.*"/>
          <match lemma="année" tags="n.*"/>
          <match lemma="lustre" tags="n.*"/>
          <match lemma="décennie" tags="n.*"/>
          <match lemma="siècle" tags="n.*"/>
          <match lemma="vacances" tags="n.*"/>
        </or>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match lemma="de" tags="pr"/>
        <repeat from="0" upto="1">
          <match tags="det.def.*"/>
        </repeat>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*.pl"/>
      </rule>
      <rule weight="1.0">
        <match lemma="le" tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="vbser.*"/>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="vblex.pp.*"/>
        </or>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
    </def-macro>

    <!-- Aquesta macro tria el verb p2 pronominal si p1 és intransitiu i p2 no pronominal si p1 és transitiu -->
    <!-- p1 = verb fra, p2 = verb oci (intransitiu > pronominal, transitiu > no pronominal) -->
    <!-- la manera de veure que el verb és transitiu és molt barroera -->
    <def-macro n="verb_intrans_trans-pron_nopron" npar="2">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2" tags="vblex.pron.*"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="2" tags="vblex.nopron.*"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="2" tags="vblex.nopron.*"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="2" tags="vblex.nopron.*"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match lemma="de" tags="pr"/>
        <repeat from="0" upto="1">
          <match tags="det.def.*"/>
        </repeat>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*.pl"/>
      </rule>
      <rule weight="1.0">
        <match lemma="le" tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*"><select plemma="2" tags="vblex.nopron.*"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="vbser.*"/>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match plemma="1" tags="vblex.pp.*"><select plemma="2" tags="vblex.nopron.*"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="vblex.pp.*"/>
        </or>
        <match plemma="1" tags="vblex.pp.*"><select plemma="2" tags="vblex.nopron.*"/></match>
      </rule>
    </def-macro>

    <!-- p1 = verb fra, p2 = verb oci no pron, p3 = verb oci pron -->
