<!-- **************** CNJADV **************** -->

  <!-- coma >> comme, que, en tant que -->
  <rule weight="0.8">
    <match lemma="coma" tags="cnjadv"><select lemma="comme"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="tal" tags="adj.*"/>
      <match lemma="tau" tags="adj.*"/>
      <match lemma="tan" tags="adv"/>
      <match lemma="tant" tags="adv"/>
      <match lemma="autan" tags="adv"/>
      <match lemma="autant" tags="adv"/>
    </or>
    <match lemma="coma" tags="cnjadv"><select lemma="que"/></match>
  </rule>

  <!-- com >> comme, que, en tant que -->
  <rule weight="0.8">
    <match lemma="com" tags="cnjadv"><select lemma="comme"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="tal" tags="adj.*"/>
      <match lemma="tau" tags="adj.*"/>
      <match lemma="tan" tags="adv"/>
      <match lemma="tant" tags="adv"/>
      <match lemma="autan" tags="adv"/>
      <match lemma="autant" tags="adv"/>
    </or>
    <match lemma="com" tags="cnjadv"><select lemma="que"/></match>
  </rule>

  <!-- per'mor que >> parce que, puisque -->
  <rule weight="0.8">
    <match lemma="per'mor que" tags="cnjadv"><select lemma="parce que"/></match>
  </rule>

  <!-- tanlèu que >> aussitôt que, sitôt que -->
  <rule weight="0.8">
    <match lemma="tanlèu que que" tags="cnjadv"><select lemma="aussitôt que"/></match>
  </rule>

</rules>
