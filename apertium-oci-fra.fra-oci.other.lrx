<rules>

<!-- **************** PREP **************** -->

  <!-- vis-à-vis de >> de cap a, au respècte de -->
  <rule weight="0.6">
    <match lemma="vis-a-vis de" tags="pr" v="oci"><select lemma="de cap a"/></match>
    <match lemma="vis-a-vis de" tags="pr" v="oci@gascon"><select lemma="au respècte de"/></match>
  </rule>

  <!-- de >> de, entà -->
  <rule weight="0.6">
    <match lemma="de" tags="pr"><select lemma="de"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="empresser" tags="vblex.*"/>
      <match lemma="presser" tags="vblex.*"/>
      <match lemma="affairer" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="de" tags="pr"><select lemma="entà"/></match>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <repeat from="0" upto="2">
      <match tags="prn.pro.*"/>
    </repeat>
    <match tags="*.inf"/>
  </rule>

  <!-- à >> a, en, de, [remove] -->
  <rule weight="0.6">
    <match lemma="à" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="commencer" tags="vblex.*"/>
      <match lemma="répugner" tags="vblex.*"/>
      <match lemma="chercher" tags="vblex.*"/>
      <match lemma="rechercher" tags="vblex.*"/>
      <match lemma="prendre# garde" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="adv"/>
        <match tags="prn.pro.*"/>
      </or>
    </repeat>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="machine" tags="n.*"/>
      <match lemma="pompe" tags="n.*"/>
    </or>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match tags="*.inf"/>
      <match tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="poudre" tags="n.*"/>
      <match lemma="chair" tags="n.*"/>
    </or>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <match lemma="canon" tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="moulin" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match lemma="vent" tags="n.*"/>
      <match lemma="huile" tags="n.*"/>
      <match lemma="eau" tags="n.*"/>
      <match lemma="café" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="bombe" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match lemma="cobalt" tags="n.*"/>
      <match lemma="hidrogène" tags="n.*"/>
      <match lemma="neutron" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="lampe" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match lemma="huile" tags="n.*"/>
      <match lemma="alcohol" tags="n.*"/>
      <match lemma="pétrole" tags="n.*"/>
      <match lemma="gaz" tags="n.*"/>
      <match lemma="néon" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="fer" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <match tags="vblex.inf"/>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="train" tags="n.*"/>
      <match lemma="avion" tags="n.*"/>
    </or>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <repeat from="0" upto="1">
      <match tags="adj.*"/>
    </repeat>
    <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="moteur" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <repeat from="0" upto="1">
      <or>
        <match tags="adj.*"/>
        <match tags="num.*"/>
        <match tags="num"/>
      </or>
    </repeat>
    <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match lemma="fragmentation" tags="n.*"/>
      <match lemma="dispersion" tags="n.*"/>
      <match lemma="sous-munition" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="gaz" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <match lemma="effet" tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="chambre" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <or>
      <match lemma="air" tags="n.*"/>
      <match lemma="combustion" tags="n.*"/>
      <match lemma="gaz" tags="n.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="chasse" tags="n.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <match tags="det.*"/>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <match tags="adj.*"/>
    <match lemma="à" tags="pr"><select lemma="de"/></match>
    <repeat from="0" upto="2">
      <match tags="prn.pro.*"/>
    </repeat>
    <match tags="*.inf"/>
  </rule>
  <rule weight="1.0">
    <match lemma="à" tags="pr"><select lemma="en"/></match>
    <or>
      <match lemma="bicyclette" tags="n.*"/>
      <match lemma="vélo" tags="n.*"/>
    </or>
  </rule>

  <!-- dans >> dins/dens, en -->
  <rule weight="0.6">
    <match lemma="dans" tags="pr"><select lemma="dins"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="dans" tags="pr"><select lemma="dens"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="dans" tags="pr"><select lemma="en"/></match>
    <match lemma="tout" tags="predet.*"/>
    <match lemma="le" tags="det.*"/>
    <match tags="np.top.*"/>
  </rule>

  <!-- en >> en, de -->
  <rule weight="0.8">
    <match lemma="en" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="en" tags="pr"><select lemma="de"/></match>
    <or>
      <seq n="matériel"/>
    </or>
  </rule>
  <rule weight="1.2">
    <or>
      <match lemma="riche" tags="adj.*"/>
      <match lemma="pauvre" tags="adj.*"/>
    </or>
    <match lemma="en" tags="pr"><select lemma="en"/></match>
    <or>
      <seq n="matériel"/>
    </or>
  </rule>

  <!-- pour >> per/entà -->
  <rule weight="0.8" v="oci">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
  </rule>
  <rule weight="0.8" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="entà"/></match>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <or>
      <match lemma="merci" tags="ij"/>
      <match lemma="remercier" tags="vblex.*"/>
      <match lemma="féliciter" tags="vblex.*"/>
    </or>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <repeat from="1" upto="10">
      <or>
        <match tags="num.*"/>
      </or>
    </repeat>
    <repeat from="0" upto="1">
      <or>
        <match lemma="de" tags="pr"/>
      </or>
    </repeat>
    <or>
      <seq n="monnaies"/>
      <seq n="poids"/>
      <seq n="capacites"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <or>
      <match lemma="cent" tags="num.*"/>
      <match lemma="mille" tags="num.*"/>
      <match lemma="million" tags="num.*"/>
      <match lemma="milliard" tags="num.*"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <or>
      <match lemma="prendre" tags="vblex.*"/>
      <match lemma="passer" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <repeat from="0" upto="4">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <match tags="n.*"/>
        <match tags="prn.*"/>
      </or>
    </repeat>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <match tags="det.*"/>
    <repeat from="0" upto="1">
      <or>
        <match tags="adj.*"/>
      </or>
    </repeat>
    <match tags="n.*"/>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <or>
      <match lemma="quitte" tags="adj.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <match lemma="ce" tags="prn.*"/>
    <match lemma="qui" tags="rel.*"/>
    <match lemma="être" tags="vbser.*"/>
    <match lemma="de" tags="pr"/>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <repeat from="0" upto="1">
      <match tags="det.def.*"/>
    </repeat>
    <or>
      <match lemma="Noël" tags="np.*"/>
      <match lemma="Pâques" tags="np.*"/>
      <match lemma="Toussaint" tags="np.*"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <match tags="det.*"/>
    <or>
      <match lemma="vacances" tags="n.*"/>
      <match lemma="réveillon" tags="n.*"/>
      <match lemma="mois" tags="n.*"/>
      <match lemma="semaine" tags="n.*"/>
      <match lemma="jour" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <or>
      <match lemma="un" tags="det.*"/>
      <match tags="det.def.*"/>
    </or>
    <or>
      <match lemma="fois" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <match tags="det.def.*"/>
    <or>
      <seq n="ordinaux"/>
    </or>
    <or>
      <match lemma="fois" tags="n.*"/>
      <match lemma="vacances" tags="n.*"/>
      <match lemma="réveillon" tags="n.*"/>
      <match lemma="mois" tags="n.*"/>
      <match lemma="semaine" tags="n.*"/>
      <match lemma="jour" tags="n.*"/>
    </or>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match tags="sent"/>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <or>
      <match lemma="moi" tags="prn.*"/>
      <match lemma="toi" tags="prn.*"/>
      <match lemma="lui" tags="prn.*"/>
      <match lemma="nous" tags="prn.*"/>
      <match lemma="vous" tags="prn.*"/>
      <match lemma="eux" tags="prn.*"/>
    </or>
    <match tags="sent"/>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <repeat from="0" upto="3">
      <or>
        <match tags="adv"/>
        <match tags="prn.pro.*"/>
      </or>
    </repeat>
    <match tags="vbhaver.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <match tags="vblex.pp.m.sg"/>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="faire" tags="vblex.*"/>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <match lemma="passer" tags="vblex.inf"/>
    <repeat from="0" upto="4">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <match tags="n.*"/>
        <match tags="prn.*"/>
      </or>
    </repeat>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
  </rule>

  <rule weight="1.0" v="oci@gascon">
    <match lemma="tenir" tags="vblex.*"/>
    <repeat from="0" upto="4">
      <or>
        <match tags="det.*"/>
        <match tags="adj.*"/>
        <match tags="n.*"/>
        <match tags="prn.*"/>
      </or>
    </repeat>
    <match lemma="pour" tags="pr"><select lemma="per"/></match>
    <match lemma="responsable"/>
  </rule>

  <!-- lors de >> durant, pendent -->
  <rule weight="0.8">
    <match lemma="lors de" tags="pr"><select lemma="durant"/></match>
  </rule>

  <!-- vers >> cap a, cap tà -->
  <rule weight="0.8" v="oci@gascon">
    <match lemma="vers" tags="pr"><select lemma="cap a"/></match>
  </rule>
  <rule weight="1.0" v="oci@gascon">
    <or>
      <match lemma="aller" tags="vblex.*"/>
