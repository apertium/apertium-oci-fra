      <match lemma="envoyer" tags="vblex.*"/>
      <match lemma="aiguiller" tags="vblex.*"/>
      <match lemma="orienter" tags="vblex.*"/>
      <match lemma="tourner" tags="vblex.*"/>
      <match lemma="suivre" tags="vblex.*"/>
      <match lemma="précéder" tags="vblex.*"/>
      <match lemma="mener" tags="vblex.*"/>
      <match lemma="amener" tags="vblex.*"/>
      <match lemma="emmener" tags="vblex.*"/>
      <match lemma="reconduire" tags="vblex.*"/>
      <match lemma="raccompagner" tags="vblex.*"/>
      <match lemma="accompagner" tags="vblex.*"/>
      <match lemma="cornaquer" tags="vblex.*"/>
      <match lemma="router" tags="vblex.*"/>
      <match lemma="détourner" tags="vblex.*"/>
      <match lemma="guider" tags="vblex.*"/>
      <match lemma="escorter" tags="vblex.*"/>
      <match lemma="convoyer" tags="vblex.*"/>
      <match lemma="filer" tags="vblex.*"/>
      <match lemma="bondir" tags="vblex.*"/>
      <match lemma="décamper" tags="vblex.*"/>
      <match lemma="détaler" tags="vblex.*"/>
      <match lemma="foncer" tags="vblex.*"/>
      <match lemma="cavaler" tags="vblex.*"/>
      <match lemma="pédaler" tags="vblex.*"/>
      <match lemma="sprinter" tags="vblex.*"/>
      <match lemma="crapahuter" tags="vblex.*"/>
      <match lemma="ramper" tags="vblex.*"/>
      <match lemma="évoluer" tags="vblex.*"/>
      <match lemma="manœuvrer" tags="vblex.*"/>
      <match lemma="progresser" tags="vblex.*"/>
      <match lemma="clopiner" tags="vblex.*"/>
      <match lemma="venir" tags="vblex.*"/>
      <match lemma="revenir" tags="vblex.*"/>
      <match lemma="retourner" tags="vblex.*"/>
      <match lemma="voyager" tags="vblex.*"/>
      <match lemma="arriver" tags="vblex.*"/>
      <match lemma="parvenir" tags="vblex.*"/>
      <match lemma="monter" tags="vblex.*"/>
      <match lemma="descendre" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="2">
      <or>
        <match tags="adv"/>
        <match tags="preadv"/>
      </or>
    </repeat>
    <match lemma="vers" tags="pr"><select lemma="cap tà"/></match>
  </rule>

  <!-- sous l'effet de >> a causa de, jos l'efièch de -->
  <!-- sous l'effet de >> pr'amor de, devath l'efèit de -->
  <rule weight="0.8">
    <match lemma="sous l'effet de" tags="pr" v="oci"><select lemma="a causa de"/></match>
    <match lemma="sous l'effet de" tags="pr" v="oci@gascon"><select lemma="pr'amor de"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="sous l'effet de" tags="pr" v="oci"><select lemma="jos l'efièch de"/></match>
    <match lemma="sous l'effet de" tags="pr" v="oci@gascon"><select lemma="devath l'efèit de"/></match>
    <repeat from="0" upto="2">
      <or>
        <match tags=""/>
        <match tags="*"/>
      </or>
    </repeat>
    <or>
      <match lemma="drogue" tags="n.*"/>
      <match lemma="alcool" tags="n.*"/>
      <match lemma="cannabis" tags="n.*"/>
      <match lemma="cocaïne" tags="n.*"/>
      <match lemma="héroïne" tags="n.*"/>
      <match lemma="hashish" tags="n.*"/>
      <match lemma="LMD" tags="n.*"/>
      <match lemma="ecstasy" tags="n.*"/>
      <match lemma="boisson" tags="n.*"/>
      <match lemma="vin" tags="n.*"/>
      <match lemma="bière" tags="n.*"/>
      <match lemma="whisky" tags="n.*"/>
      <match lemma="champagne" tags="n.*"/>
      <match lemma="apéritif" tags="n.*"/>
      <match lemma="liqueur" tags="n.*"/>
      <match lemma="eau-de-vie" tags="n.*"/>
    </or>
  </rule>


<!-- **************** REL **************** -->

  <!-- qui rel >> que, qui -->
  <rule weight="0.8">
    <match lemma="qui" tags="rel.*" v="oci"><select lemma="que"/></match>
    <match lemma="qui" tags="rel.*" v="oci@gascon"><select lemma="qui"/></match>
  </rule>

  <!-- celui que >> lo que, lo qui -->
  <rule weight="0.8">
    <match lemma="celui que" tags="rel.*" v="oci"><select lemma="lo que"/></match>
    <match lemma="celui que" tags="rel.*" v="oci@gascon"><select lemma="lo qui"/></match>
  </rule>

  <!-- celui qui >> lo que, lo qui -->
  <rule weight="0.8">
    <match lemma="celui qui" tags="rel.*" v="oci"><select lemma="lo que"/></match>
    <match lemma="celui qui" tags="rel.*" v="oci@gascon"><select lemma="lo qui"/></match>
  </rule>

  <!-- ceux et celles que >> los e las que, los e las qui -->
  <rule weight="0.8">
    <match lemma="ceux et celles que" tags="rel.*" v="oci"><select lemma="los e las que"/></match>
    <match lemma="ceux et celles que" tags="rel.*" v="oci@gascon"><select lemma="los e las qui"/></match>
  </rule>

  <!-- celles et ceux que >> la e lo que, las e los qui -->
  <rule weight="0.8">
    <match lemma="celles et ceux que" tags="rel.*" v="oci"><select lemma="las e los que"/></match>
    <match lemma="celles et ceux que" tags="rel.*" v="oci@gascon"><select lemma="las e los qui"/></match>
  </rule>

  <!-- ceux et celles qui >> los e las que, los e las qui -->
  <rule weight="0.8">
    <match lemma="ceux et celles qui" tags="rel.*" v="oci"><select lemma="los e las que"/></match>
    <match lemma="ceux et celles qui" tags="rel.*" v="oci@gascon"><select lemma="los e las qui"/></match>
  </rule>

  <!-- celles et ceux qui >> las e los que, las e los qui -->
  <rule weight="0.8">
    <match lemma="celles et ceux qui" tags="rel.*" v="oci"><select lemma="las e los que"/></match>
    <match lemma="celles et ceux qui" tags="rel.*" v="oci@gascon"><select lemma="las e los qui"/></match>
  </rule>

  <!-- où -> où, que -->
  <rule weight="0.8">
    <match lemma="où" tags="rel.*" v="oci"><select lemma="ont"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="heure"/>
      <match lemma="moment"/>
      <match lemma="jour"/>
      <match lemma="année"/>
      <match lemma="mois"/>
      <match lemma="minute"/>
      <match lemma="instant"/>
    </or>
    <match lemma="où" tags="rel.*" v="oci"><select lemma="que"/></match>
  </rule>


<!-- **************** CNJSUB **************** -->

  <!-- que cnjsub >> que -->
  <rule weight="0.8">
    <match lemma="que" tags="cnjsub"><select lemma="que"/></match>
  </rule>

