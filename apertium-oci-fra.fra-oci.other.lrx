<!-- **************** PRN **************** -->

  <!-- combien prn.itg >> quant -->
  <rule weight="0.8">
    <match lemma="combien" tags="prn.itg.*"><select lemma="quant" tags="prn.itg.m.sg"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="combien" tags="prn.itg.*"><select lemma="quant" tags="prn.itg.m.pl"/></match>
    <match tags="vbser.*.pl"/>
  </rule>
  <rule weight="1.2">
    <match lemma="combien" tags="prn.itg.*"><select lemma="quant" tags="prn.itg.f.pl"/></match>
    <match tags="vbser.*.pl"/>
    <match tags="det.*.f.pl"/>
  </rule>
  <rule weight="1.2">
    <match lemma="combien" tags="prn.itg.*"><select lemma="quant" tags="prn.itg.f.pl"/></match>
    <match tags="vbser.*.pl"/>
    <match tags="det.*.mf.pl"/>
    <or>
      <match tags="n.*.f.pl"/>
      <match tags="adj.*.f.pl"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="combien" tags="prn.itg.*"><select lemma="quant" tags="prn.itg.m.pl"/></match>
    <repeat from="0" upto="5">
      <or>
        <match tags="prn.tn.*"/>
        <match tags="det.*"/>
        <match tags="n.*"/>
        <match tags="adj.*"/>
        <match tags="adv"/>
        <match tags="preadv"/>
        <match tags="pr"/>
      </or>
    </repeat>
    <match lemma="en" tags="prn.pro.adv"/>
    <or>
      <match tags="vblex.*"/>
      <match tags="vbhaver.*"/>
      <match tags="vbmod.*"/>
    </or>
  </rule>

  <!-- lui prn.pro >> li, lo prn.pro.p3.*.sg (lengadocian) -->
  <rule weight="0.8" v="oci">
    <match lemma="lui" tags="prn.pro.*"><select lemma="li"/></match>
  </rule>
  <rule weight="1.0" v="oci">
    <match lemma="lui" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.p3.m.sg"/></match>
    <or>
      <match lemma="rendre# visite" tags="vblex.*"/>
      <match lemma="faire# face" tags="vblex.*"/>
      <match lemma="décerner# un prix" tags="vblex.*"/>
      <match lemma="donner# la priorité" tags="vblex.*"/>
      <match lemma="mettre# son veto" tags="vblex.*"/>
      <match lemma="mettre# son véto" tags="vblex.*"/>
    </or>
  </rule>
  <rule weight="1.0" v="oci">
    <match lemma="lui" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.p3.m.sg"/></match>
    <match tags="vbhaver.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <or>
      <match lemma="rendre# visite" tags="vblex.*"/>
      <match lemma="faire# face" tags="vblex.*"/>
      <match lemma="décerner# un prix" tags="vblex.*"/>
      <match lemma="donner# la priorité" tags="vblex.*"/>
      <match lemma="mettre# son veto" tags="vblex.*"/>
      <match lemma="mettre# son véto" tags="vblex.*"/>
    </or>
  </rule>

  <!-- lui prn.pro >> lo prn.pro.p3.m.sg, lo prn.pro.p3.f.sg, ac (Gascon) -->
  <rule weight="0.8" v="oci@gascon">
    <match lemma="lui" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.p3.m.sg"/></match>
  </rule>
  <rule weight="1.0" v="oci@gascon">
    <match lemma="le" tags="prn.pro.*"/>
    <match lemma="lui" tags="prn.pro.*"><select lemma="ac" tags="prn.pro.*"/></match>
  </rule>

  <!-- leur prn.pro >> li, lo prn.pro.p3.*.pl (lengadocian) -->
  <rule weight="0.8" v="oci">
    <match lemma="leur" tags="prn.pro.*"><select lemma="li"/></match>
  </rule>
  <rule weight="1.0" v="oci">
    <match lemma="leur" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.p3.m.pl"/></match>
    <or>
      <match lemma="rendre# visite" tags="vblex.*"/>
      <match lemma="faire# face" tags="vblex.*"/>
      <match lemma="décerner# un prix" tags="vblex.*"/>
      <match lemma="donner# la priorité" tags="vblex.*"/>
      <match lemma="mettre# son veto" tags="vblex.*"/>
      <match lemma="mettre# son véto" tags="vblex.*"/>
    </or>
  </rule>
  <rule weight="1.0" v="oci">
    <match lemma="lui" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.p3.m.pl"/></match>
    <match tags="vbhaver.*"/>
    <repeat from="0" upto="2">
      <match tags="adv"/>
    </repeat>
    <or>
      <match lemma="rendre# visite" tags="vblex.*"/>
      <match lemma="faire# face" tags="vblex.*"/>
      <match lemma="décerner# un prix" tags="vblex.*"/>
      <match lemma="donner# la priorité" tags="vblex.*"/>
      <match lemma="mettre# son veto" tags="vblex.*"/>
      <match lemma="mettre# son véto" tags="vblex.*"/>
    </or>
  </rule>

  <!-- leur prn.pro >> lo prn.pro.p3.mf.pl, prn.pro.p3.f, ac (Gascon) -->
  <rule weight="0.8" v="oci@gascon">
    <match lemma="leur" tags="prn.pro.*"><select lemma="lo" tags="prn.pro.*"/></match>
  </rule>
  <rule weight="1.0" v="oci@gascon">
    <match lemma="le" tags="prn.pro.*"/>
    <match lemma="leur" tags="prn.pro.*"><select lemma="ac" tags="prn.pro.*"/></match>
  </rule>

  <!-- lui prn.enc >> lo prn.enc.p3.m.sg, lo prn.enc.p3.f.sg, ac prn.enc (Gascon) -->
  <rule weight="0.8" v="oci@gascon">
    <match lemma="lui" tags="prn.enc.*"><select lemma="lo" tags="prn.enc.p3.m.sg"/></match>
  </rule>
  <rule weight="1.0" v="oci@gascon">
    <match lemma="le" tags="prn.enc.*"/>
    <match lemma="lui" tags="prn.enc.*"><select lemma="ac" tags="prn.enc.*"/></match>
  </rule>

  <!-- leur prn.enc >> lo prn.enc.p3.m, lo prn.enc.p3.f (Gascon) -->
  <rule weight="0.8" v="oci@gascon">
    <match lemma="leur" tags="prn.enc.*"><select lemma="lo" tags="prn.enc.*"/></match>
  </rule>
  <rule weight="1.0" v="oci@gascon">
    <match lemma="le" tags="prn.enc.*"/>
    <match lemma="leur" tags="prn.enc.*"><select lemma="ac" tags="prn.enc.*"/></match>
  </rule>

</rules>
