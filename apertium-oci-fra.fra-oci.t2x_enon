    <rule comment="RÈGLA: SPronPro SPronPro SV shens">
      <pattern>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SV"/>
        <pattern-item n="shens_adv"/>
      </pattern>
