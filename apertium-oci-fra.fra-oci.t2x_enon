    </rule>

    <rule comment="RÈGLA: SENT">
      <pattern>
        <pattern-item n="SENT"/>
      </pattern>
      <action>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="oc"/></let>
      </action>
    </rule>

    <!-- RÈGLAS PER GENERAR UN ENONCIATIU -->

    <!-- 1. Règles de "courte portée", qui analysent tout juste le verbe -->

    <rule comment="RÈGLA: SV">
      <pattern>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="apondre_enonciatiu2"><with-param pos="1"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <rule comment="RÈGLA: SPronPro SV">
      <pattern>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="apondre_enonciatiu2"><with-param pos="2"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <rule comment="RÈGLA: SPronPro SPronPro SV">
      <pattern>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="apondre_enonciatiu2"><with-param pos="3"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <rule comment="RÈGLA: SPronPro SPronPro SPronPro SV">
      <pattern>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <call-macro n="apondre_enonciatiu2"><with-param pos="4"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b pos="2"/>
          <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="lem"/>
            <clip pos="4" part="tags"/>
            <clip pos="4" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <!-- regles pour les frases de type: je ne veux pas que manger -> que vòli shens que minjar (ici on génère 'que' au lieu de 'ne') -->

    <rule comment="RÈGLA: SV shens_adv">
      <pattern>
        <pattern-item n="SV"/>
