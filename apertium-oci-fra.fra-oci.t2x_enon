  <section-def-macros>
    <def-macro n="f_bcond" npar="2"><!--Per mirar si el blanc conté o no format. Cal posar aquesta funció en les regles en què desaparegui alguna paraula, per tal de decidir si el blanc de la paraula eliminada s'ha d'eliminar o conservar. Si conté format cal conservar-la, si no en conté cal eliminar-la perquè no apareguin dos espais seguits. -->
      <choose>
        <when>
          <test>
            <not>
              <equal>
                <b pos="1"/>
                <lit v=" "/>
              </equal>
            </not>
          </test>
          <out>
            <b pos="1"/>
          </out>
        </when>
      </choose>
    </def-macro>

    <def-macro n="escriure_enonciatiu3" npar="3"><!-- par1 = vèrb, par2 = sent (per causir l'enonciatiu), par3 = mot que seguís lo possible enonciatiu (per la gestion de las majusculas) -->
      <choose>
        <when>
          <test>
            <equal><clip pos="1" part="negation"/><lit-tag v="negation"/></equal>
          </test>
          <out>
            <chunk>
              <get-case-from pos="3">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="ne"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </when>
        <when>
          <test>
            <equal><clip pos="2" part="lem"/><lit v="quest"/></equal>
          </test>
          <out>
            <chunk>
              <get-case-from pos="3">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="e"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </when>
        <when>
          <test>
            <equal><clip pos="2" part="lem"/><lit v="excl"/></equal>
          </test>
          <out>
            <chunk>
              <get-case-from pos="3">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="be"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </when>
        <otherwise>
          <out>
            <chunk>
              <get-case-from pos="3">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="que"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </otherwise>
      </choose>
      <modify-case><clip pos="3" part="lem"/><lit v="aa"/></modify-case>
    </def-macro>

    <def-macro n="escriure_enonciatiu2" npar="2"><!-- par1 = vèrb, par2 = mot que seguís lo possible enonciatiu (per la gestion de las majusculas) -->
      <choose>
        <when>
          <test>
            <equal><clip pos="1" part="negation"/><lit-tag v="negation"/></equal>
          </test>
          <out>
            <chunk>
              <get-case-from pos="2">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="ne"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </when>
        <otherwise>
          <out>
            <chunk>
              <get-case-from pos="2">
                <lit v="enon"/>
              </get-case-from>
              <lit-tag v="SAdv"/>
              <lit v="{^"/>
              <lit v="que"/>
              <lit-tag v="adv"/>
              <lit-tag v="enon"/>
              <lit v="$}"/>
            </chunk>
            <b/>
            <b/>	<!-- extra blank: workaround space bug -->
          </out>
        </otherwise>
      </choose>
      <modify-case><clip pos="2" part="lem"/><lit v="aa"/></modify-case>
    </def-macro>

    <def-macro n="aprondre_enonciatiu3" npar="3"><!-- par1 = vèrb, par2 = sent (per causir l'enonciatiu), par3 = mot que seguís lo possible enonciatiu (per la gestion de las majusculas) -->
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" part="negation"/><lit-tag v="negation"/></equal>
              <not>
                <equal><clip pos="1" part="temps"/><lit-tag v="inf"/></equal>
              </not>
            </and>
          </test>
          <let><var n="set_enonciatiu"/><lit v="oc"/></let>
          <call-macro n="escriure_enonciatiu3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" part="temps"/><lit-tag v="inf"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="ger"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="pp"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="imp"/></equal>
            </or>
          </test>
          <let><var n="set_enonciatiu"/><lit v="non"/></let>
        </when>
        <otherwise>
          <let><var n="set_enonciatiu"/><lit v="oc"/></let>
          <call-macro n="escriure_enonciatiu3"><with-param pos="1"/><with-param pos="2"/><with-param pos="3"/></call-macro>
        </otherwise>
      </choose>
    </def-macro>

    <def-macro n="apondre_enonciatiu2" npar="2"><!-- par1 = vèrb, par2 = mot que seguís lo possible enonciatiu (per la gestion de las majusculas) -->
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" part="negation"/><lit-tag v="negation"/></equal>
              <not>
                <equal><clip pos="1" part="temps"/><lit-tag v="inf"/></equal>
              </not>
            </and>
          </test>
          <let><var n="set_enonciatiu"/><lit v="oc"/></let>
          <call-macro n="escriure_enonciatiu2"><with-param pos="1"/><with-param pos="2"/></call-macro>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" part="temps"/><lit-tag v="inf"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="ger"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="pp"/></equal>
              <equal><clip pos="1" part="temps"/><lit-tag v="imp"/></equal>
            </or>
          </test>
          <let><var n="set_enonciatiu"/><lit v="non"/></let>
        </when>
        <otherwise>
          <let><var n="set_enonciatiu"/><lit v="oc"/></let>
          <call-macro n="escriure_enonciatiu2"><with-param pos="1"/><with-param pos="2"/></call-macro>
        </otherwise>
      </choose>
    </def-macro>

  </section-def-macros>

