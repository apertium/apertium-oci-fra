        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <!-- 2. Règles de "longue portée", qui analysent du verbe jusqu'à la fin de la phrase  -->

    <!-- sens proclitic -->

    <rule comment="RÈGLA: SV SENT">
      <pattern>
        <pattern-item n="SV"/>
        <pattern-item n="SENT"/>
      </pattern>
      <action>
        <call-macro n="aprondre_enonciatiu3"><with-param pos="1"/><with-param pos="2"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
