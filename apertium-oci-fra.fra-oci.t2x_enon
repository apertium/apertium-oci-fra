    <rule comment="RÈGLA: SPronPro SV shens">
      <pattern>
        <pattern-item n="SPronPro"/>
        <pattern-item n="SV"/>
        <pattern-item n="shens_adv"/>
      </pattern>
      <action>
        <let><clip pos="2" part="negation"/><lit-tag v="shens"/></let>
