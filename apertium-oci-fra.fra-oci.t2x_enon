            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
        </out>
        <let><var n="inici"/><lit-tag v="non"/></let>
      </action>
    </rule>

    <rule comment="RÈGLA: SV SN_SA_SAdv_UNK SENT: La maison est belle. > L'ostau qu'ei beròi.">
      <pattern>
        <pattern-item n="SV"/>
        <pattern-item n="SN_SA_SAdv_UNK"/>
        <pattern-item n="SENT"/>
      </pattern>
      <action>
        <call-macro n="aprondre_enonciatiu3"><with-param pos="1"/><with-param pos="3"/><with-param pos="1"/></call-macro>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
  </out>	<!-- extra blank: workaround space bug -->
<choose>
  <when>
    <test>
      <equal><var n="set_enonciatiu"/><lit v="oc"/></equal>
    </test>
  <out>
    <b/>
  </out>
  </when>
</choose>
  <out>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
