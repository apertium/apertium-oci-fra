<?xml version="1.0" encoding="UTF-8"?>
<lrx>
  <def-macros>
    <!-- Aquesta macro tria el verb p2 si p1 és intransitiu i p3 si p1 és transitiu -->
    <!-- p1 = verb fra, p2 = verb cat intransitiu, p3 = verb cat transitiu -->
    <!-- la manera de veure que el verb és transitiu és molt barroera -->
    <def-macro n="verb_intrans_trans" npar="3">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.2">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match tags="det.*"/>
        <or>
          <seq n="jorns"/>
          <seq n="meses"/>
          <seq n="sasons"/>
          <seq n="parts_jorn"/>
          <match lemma="velha" tags="n.*"/>
          <match lemma="vegelha" tags="n.*"/>
          <match lemma="vigília" tags="n.*"/>
          <match lemma="dimenjada" tags="n.*"/>
          <match tags="num"/>
        </or>
      </rule>
      <rule weight="1.4">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match tags="det.*"/>
        <or>
          <seq n="jorns"/>
          <seq n="meses"/>
          <seq n="sasons"/>
          <seq n="parts_jorn"/>
          <match lemma="velha" tags="n.*"/>
          <match lemma="vegelha" tags="n.*"/>
          <match lemma="vigília" tags="n.*"/>
          <match lemma="dimenjada" tags="n.*"/>
          <match tags="num"/>
        </or>
        <repeat from="0" upto="1">
          <match tags="adj.*"/>
        </repeat>
        <or>
          <match tags="det.*"/>
          <match tags="np.*"/>
        </or>
      </rule>
      <rule weight="1.0">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match tags="num"/>
        </or>
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0" v="oci">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match lemma="de" tags="pr"/>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*.pl"/>
      </rule>
      <rule weight="1.0" v="oci@gascon">
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
        <repeat from="0" upto="3">
          <or>
            <match tags="preadv"/>
            <match tags="adv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match tags="n.*"/>
      </rule>
      <rule weight="1.0" v="oci">
        <match lemma="lo" tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0" v="oci">
        <match lemma="lo" tags="prn.pro.*"/>
        <match tags="prn.pro.*"/>
        <match plemma="1" tags="vblex.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="vbser.*"/>
        <repeat from="0" upto="2">
          <match tags="adv"/>
        </repeat>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="vblex.pp.*"/>
        </or>
        <match plemma="1" tags="vblex.pp.*"><select plemma="3"/></match>
      </rule>
    </def-macro>

