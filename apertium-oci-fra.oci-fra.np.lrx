          <match lemma="règne" tags="n.*"/>
          <match lemma="regéncia" tags="n.*"/>
          <match lemma="vicereialtat" tags="n.*"/>
          <match lemma="vicereiautat" tags="n.*"/>
        </or>
        <match lemma="de" tags="pr"/>
        <repeat from="0" upto="2">
           <match tags="np.ant.*"/>
        </repeat>
        <match plemma="1" tags="np.*"><select plemma="2"/></match>
      </rule>
      <rule weight="2.0">
        <match plemma="1" tags="np.*"><select plemma="2"/></match>
        <repeat from="0" upto="2">
           <match tags="np.ant.*"/>
        </repeat>
        <or>
          <match lemma="regnar" tags="vblex.*"/>
          <match lemma="governar" tags="vblex.*"/>
        </or>
      </rule>
