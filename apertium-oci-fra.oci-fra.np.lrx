  <!-- Comminge >> pays de Comminges, Comminges -->
  <rule weight="0.8">
    <match lemma="Comenge" tags="np.top.*"><select lemma="pays de Comminges"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="adj.*"/>
    <match lemma="Comenge" tags="np.top.*"><select lemma="Comminges"/></match>
