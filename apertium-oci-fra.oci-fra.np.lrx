<lrx>
  <def-macros>
    <def-macro n="noms_propres" npar="2">
      <rule weight="0.6">
        <match plemma="1" tags="np.*"><select plemma="1"/></match>
      </rule>
      <rule weight="2.0">
        <match plemma="1" tags="np.*"><select plemma="2"/></match>
        <repeat from="0" upto="5">
           <match tags="np.ant.*"/>
        </repeat>
        <match lemma="de" tags="pr"/>
        <or>
          <match tags="np.top.*"/>
          <match lemma="Borbon" tags="np.cog.*"/>
          <match lemma="Borbon-Parma" tags="np.cog.*"/>
          <match lemma="Borbon-Sicílias" tags="np.cog.*"/>
          <match lemma="Medicis" tags="np.cog.*"/>
          <match lemma="Medici" tags="np.cog.*"/>
          <match tags="np.loc"/>
        </or>
      </rule>
      <rule weight="2.0">
        <match plemma="1" tags="np.*"><select plemma="2"/></match>
        <repeat from="0" upto="2">
           <match tags="np.ant.*"/>
        </repeat>
        <match tags="det.*"/>
        <match tags="adj.*"/>
      </rule>
      <rule weight="2.0">
        <match plemma="1" tags="np.*"><select plemma="2"/></match>
        <repeat from="0" upto="2">
           <match tags="np.ant.*"/>
        </repeat>
        <or>
          <match tags="num.*"/>
          <match lemma="I" tags="adj.*"/>
          <match lemma="II" tags="adj.*"/>
          <match lemma="III" tags="adj.*"/>
