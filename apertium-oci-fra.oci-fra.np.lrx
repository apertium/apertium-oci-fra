    <!-- Aquesta macro tria el p2 per traduir el prenom p1, però manté p1 si va seguit de cognom (o paraula desconeguda) -->
    <def-macro n="ant_antcog" npar="2">
      <rule weight="0.6">
         <match plemma="1" tags="np.ant.*"><select plemma="2"/></match>
      </rule>
      <rule weight="2.0">
         <match plemma="1" tags="np.ant.*"><select plemma="1"/></match>
         <or>
           <match tags="np.*"/>
           <match tags=""/>
         </or>
      </rule>
      <rule weight="1.0">
         <or>
           <match lemma="senher" tags="n.*"/>
           <match lemma="monsur" tags="n.*"/>
           <match lemma="mossen" tags="n.*"/>
           <match lemma="mossenh" tags="n.*"/>
           <match lemma="senher" tags="n.*"/>
           <match lemma="dòna" tags="n.*"/>
           <match lemma="dauna" tags="n.*"/>
           <match lemma="dama" tags="n.*"/>
           <match lemma="madama" tags="n.*"/>
           <match lemma="madomaisèla" tags="n.*"/>
           <match lemma="madamaisèla" tags="n.*"/>
         </or>
         <match plemma="1" tags="np.*"><select plemma="1"/></match>
      </rule>
    </def-macro>
  </def-macros>

