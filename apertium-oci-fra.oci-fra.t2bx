            <call-macro n="f_bcond">
              <with-param pos="2"/>
              <with-param pos="3"/>
            </call-macro>
            <let><var n="v_det_pron_neg"/><lit v="false"/></let>
          </when>
          <otherwise>
            <out>
              <b pos="2"/>
              <chunk>
                <clip pos="3" part="whole"/>
              </chunk>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="RÈGLA: SPron_pro SPron_pro SV_pers pas (la li cantes pas > NE la lui chantes pas)">
      <pattern>
        <pattern-item n="SPron_pro"/>
        <pattern-item n="SPron_pro"/>
        <pattern-item n="SV_pers"/>
        <pattern-item n="pas"/>
      </pattern>
