            </get-case-from>
            <lit-tag v="prn.subj"/>
            <clip pos="3" part="pers"/>
            <lit-tag v="GD"/>
            <clip pos="3" part="nbr"/>
            <lit v="{^"/>
            <get-case-from pos="2">
		      <lit v="prpers"/>
