 <def-macro n="gen_subj1" npar="1">  <!--ATENCIÓN: si se crean nuevos chunks verbales en el t1x con nuevos lemas, hay que añadirlos a la lista SV_personals en el caso de que sean SV con formas personales. De lo contrario no se creará el sujeto correspondiente cuando aparezcan en un texto. -->
   <choose>
     <when>
       <test>
         <or>
           <in caseless="yes"><clip pos="1" part="lem"/><list n="SV_personals"/></in>
           <equal><clip pos="1" part="pers"/><lit-tag v="p1"/></equal>
           <equal><clip pos="1" part="pers"/><lit-tag v="p2"/></equal>
           <equal><clip pos="1" part="pers"/><lit-tag v="p3"/></equal>
         </or>
       </test>
        <choose>
          <when>
            <test>
              <or>
                <equal><var n="finfrasa"/><lit v="yes"/></equal>
                <equal><clip pos="1" part="pers"/><lit-tag v="p1"/></equal>
                <equal><clip pos="1" part="pers"/><lit-tag v="p2"/></equal>
                <equal caseless="yes"><clip pos="1" part="lem"/><lit v="falloir"/></equal>
              </or>
            </test>
            <choose>
              <when>
                <test>
                  <not>
                    <equal><clip pos="1" part="temps"/><lit-tag v="imp"/></equal>
                  </not>
                </test>
                <choose>
                  <when>
                    <test>
                      <equal caseless="yes"><clip pos="1" part="lem"/><lit v="être# vrai"/></equal>
                    </test>
                    <call-macro n="gen_ce_incond"><with-param pos="1"/></call-macro>
                  </when>
                  <otherwise>
                    <out>
                      <chunk>
                        <get-case-from pos="1">
                          <lit v="subj"/>
                        </get-case-from>
                        <lit-tag v="prn.subj"/>
                        <clip pos="1" part="pers"/>
                        <lit-tag v="GD"/>
                        <clip pos="1" part="nbr"/>
                        <lit v="{^"/>
                        <get-case-from pos="1">
                          <lit v="prpers"/>
                        </get-case-from>
                        <lit-tag v="prn.tn.3.4.5"/>
                        <lit v="$}"/>
                      </chunk>
                      <b/>
                    </out>
                  </otherwise>
                </choose>
                <choose>
                  <when>
                    <test>
                      <equal><case-of pos="1" part="lem"/><lit v="Aa"/></equal>
                    </test>
                    <modify-case><clip pos="1" part="lem"/><lit v="aa"/></modify-case>
                  </when>
                </choose>
              </when>
            </choose>
          </when>
        </choose>
     </when>
   </choose>
 </def-macro>

