    <rule comment="REGLA: cnjsub_adv SV (que cantes -> que TU chantes)">
      <pattern>
        <pattern-item n="cnjsub_adv"/>
        <pattern-item n="SV_pers"/>
      </pattern>
      <action>
        <out>
          <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <get-case-from pos="2">
              <lit v="subj"/>
            </get-case-from>
            <lit-tag v="prn.subj"/>
            <clip pos="2" part="pers"/>
            <lit-tag v="GD"/>
            <clip pos="2" part="nbr"/>
            <lit v="{^"/>
            <get-case-from pos="2">
		      <lit v="prpers"/>
            </get-case-from>
            <lit-tag v="prn.tn.3.4.5"/>
            <lit v="$}"/>
          </chunk>
          <b/>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
        </out>
        <let><var n="fifrase"/><lit v="no"/></let>
      </action>
    </rule>

    <rule comment="REGLA: cnjsub_adv SV (que li cantes -> que TU lui chantes)">
      <pattern>
